SQL
MERGE INTO &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL UT
USING (
SELECT
      SVC_CAS_ISA_FILE_ID
     ,SVC_CAS_GS_GRP_SEQ_NO
     ,SVC_CAS_ST_SEQ_NO
     ,SVC_CAS_LX_SEQ_NO
     ,SVC_CAS_CLP_CLM_SEQ_NO
     ,SVC_CAS_SVC_SEQ_NO
     ,SVC_CAS_SEQ_NO
     ,SVC_CAS_EPP_GEN_SEQ
     ,SVC_SCENARIO
     ,SVC_CONTR_PAYOR_IND
     ,SVC_CAS_CLM_ADJ_GRP_CD1
     ,SVC_CAS_CLM_ADJ_RSN_CD1
     ,SVC_CAS_CLM_ADJ_RSN_CD2
     ,SVC_CAS_CLM_ADJ_RSN_CD3
     ,SVC_CAS_CLM_ADJ_RSN_CD4
     ,SVC_CAS_CLM_ADJ_RSN_CD5
     ,SVC_CAS_CLM_ADJ_RSN_CD6
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL
ON    CLP_ISA_FILE_ID           = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO         = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO             = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD           = '   '
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL
ON    SVC_ISA_FILE_ID           = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO         = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO             = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO             = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO        = CLP_CLM_SEQ_NO
  AND SVC_SCENARIO              = '4'
  AND SVC_EXCL_RSN_CD           = '   '
INNER JOIN
      &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL
ON    SVC_CAS_ISA_FILE_ID       = SVC_ISA_FILE_ID
  AND SVC_CAS_GS_GRP_SEQ_NO     = SVC_GS_GRP_SEQ_NO
  AND SVC_CAS_ST_SEQ_NO         = SVC_ST_SEQ_NO
  AND SVC_CAS_LX_SEQ_NO         = SVC_LX_SEQ_NO
  AND SVC_CAS_CLP_CLM_SEQ_NO    = SVC_CLP_CLM_SEQ_NO
  AND SVC_CAS_SVC_SEQ_NO        = SVC_SEQ_NO
  AND SVC_CAS_EXCL_RSN_CD       = '   '
WHERE ST_EXCL_RSN_CD            = '   '
-*  AND NOT EXISTS
-*    ( SELECT 1
-*      FROM  &NA.EVAL&NA_TBL.EVAL
-*      WHERE NA_ISA_FILE_ID      = ST_ISA_FILE_ID
-*        AND NA_GS_GRP_SEQ_NO    = ST_GS_GRP_SEQ_NO
-*        AND NA_ST_SEQ_NO        = ST_SEQ_NO
-*        AND NA_N1_ENTITY_ID_CD  = 'PE '
-*        AND ((  NA_N1_ID_CD_QLFR = 'XX'
-*            AND RTRIM(NA_N1_ID_CD) = '1336615988')
-*           OR ( NA_N1_ID_CD_QLFR = 'FI'
-*            AND RTRIM(NA_N1_ID_CD) = '900939967')))
  AND EXISTS
    ( SELECT 1
      FROM   &BAS.EVAL&BAS_TBL.EVAL
      WHERE  BAS_SCENARIO         = SVC_SCENARIO
        AND  RTRIM(BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
        AND  BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
        AND  RTRIM(BAS_CHG_IND)   = 'MOD'
        AND (   RTRIM(BAS_RSN_CD) = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD1)
             OR RTRIM(BAS_RSN_CD) = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD2)
             OR RTRIM(BAS_RSN_CD) = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD3)
             OR RTRIM(BAS_RSN_CD) = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD4)
             OR RTRIM(BAS_RSN_CD) = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD5)
             OR RTRIM(BAS_RSN_CD) = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD6)))
ORDER BY 1,2,3,4,5,6,7
) ST
ON  ( UT.SVC_CAS_ISA_FILE_ID    = ST.SVC_CAS_ISA_FILE_ID
  AND UT.SVC_CAS_GS_GRP_SEQ_NO  = ST.SVC_CAS_GS_GRP_SEQ_NO
  AND UT.SVC_CAS_ST_SEQ_NO      = ST.SVC_CAS_ST_SEQ_NO
  AND UT.SVC_CAS_LX_SEQ_NO      = ST.SVC_CAS_LX_SEQ_NO
  AND UT.SVC_CAS_CLP_CLM_SEQ_NO = ST.SVC_CAS_CLP_CLM_SEQ_NO
  AND UT.SVC_CAS_SVC_SEQ_NO     = ST.SVC_CAS_SVC_SEQ_NO
  AND UT.SVC_CAS_SEQ_NO         = ST.SVC_CAS_SEQ_NO
  AND UT.SVC_CAS_EPP_GEN_SEQ    = ST.SVC_CAS_EPP_GEN_SEQ )
WHEN MATCHED THEN
UPDATE SET UT.SVC_CAS_EXCL_RSN_CD = 'C04'
;
END
-RUN
