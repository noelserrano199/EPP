SQL
INSERT INTO &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL
SELECT
      SVC_CAS_ISA_FILE_ID
     ,SVC_CAS_GS_GRP_SEQ_NO
     ,SVC_CAS_ST_SEQ_NO
     ,SVC_CAS_LX_SEQ_NO
     ,SVC_CAS_CLP_CLM_SEQ_NO
     ,SVC_CAS_SVC_SEQ_NO
     ,SVC_CAS_SEQ_NO
     ,TO_CHAR(TO_NUMBER(SVC_CAS_EPP_GEN_SEQ)+1)           AS SVC_CAS_EPP_GEN_SEQ
     ,COALESCE( RTRIM(B1.BAS_NEW_GRP_CD),RTRIM(B2.BAS_NEW_GRP_CD),RTRIM(B3.BAS_NEW_GRP_CD)
               ,RTRIM(B4.BAS_NEW_GRP_CD),RTRIM(B5.BAS_NEW_GRP_CD),RTRIM(B6.BAS_NEW_GRP_CD))
                                                          AS SVC_CAS_CLM_ADJ_GRP_CD1
     ,COALESCE(B1.BAS_NEW_RSN_CD,SVC_CAS_CLM_ADJ_RSN_CD1) AS SVC_CAS_CLM_ADJ_RSN_CD1
     ,SVC_CAS_CLM_ADJ_AMT1
     ,SVC_CAS_CLM_ADJ_QTY1
     ,COALESCE(B2.BAS_NEW_RSN_CD,SVC_CAS_CLM_ADJ_RSN_CD2) AS SVC_CAS_CLM_ADJ_RSN_CD2
     ,SVC_CAS_CLM_ADJ_AMT2
     ,SVC_CAS_CLM_ADJ_QTY2
     ,COALESCE(B3.BAS_NEW_RSN_CD,SVC_CAS_CLM_ADJ_RSN_CD3) AS SVC_CAS_CLM_ADJ_RSN_CD3
     ,SVC_CAS_CLM_ADJ_AMT3
     ,SVC_CAS_CLM_ADJ_QTY3
     ,COALESCE(B4.BAS_NEW_RSN_CD,SVC_CAS_CLM_ADJ_RSN_CD4) AS SVC_CAS_CLM_ADJ_RSN_CD4
     ,SVC_CAS_CLM_ADJ_AMT4
     ,SVC_CAS_CLM_ADJ_QTY4
     ,COALESCE(B5.BAS_NEW_RSN_CD,SVC_CAS_CLM_ADJ_RSN_CD5) AS SVC_CAS_CLM_ADJ_RSN_CD5
     ,SVC_CAS_CLM_ADJ_AMT5
     ,SVC_CAS_CLM_ADJ_QTY5
     ,COALESCE(B6.BAS_NEW_RSN_CD,SVC_CAS_CLM_ADJ_RSN_CD6) AS SVC_CAS_CLM_ADJ_RSN_CD5
     ,SVC_CAS_CLM_ADJ_AMT6
     ,SVC_CAS_CLM_ADJ_QTY6
     ,'C04'                                     AS SVC_CAS_INCL_RSN_CD
     ,'   '                                     AS SVC_CAS_EXCL_RSN_CD
     ,SVC_CAS_PATIENT_PYMT
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL
ON    CLP_ISA_FILE_ID         = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO       = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO           = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD         = '   '
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL
ON    SVC_ISA_FILE_ID         = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO       = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO           = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO           = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO      = CLP_CLM_SEQ_NO
  AND SVC_SCENARIO            = '4'
  AND SVC_EXCL_RSN_CD         = '   '
INNER JOIN
      &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL
ON    SVC_CAS_ISA_FILE_ID     = SVC_ISA_FILE_ID
  AND SVC_CAS_GS_GRP_SEQ_NO   = SVC_GS_GRP_SEQ_NO
  AND SVC_CAS_ST_SEQ_NO       = SVC_ST_SEQ_NO
  AND SVC_CAS_LX_SEQ_NO       = SVC_LX_SEQ_NO
  AND SVC_CAS_CLP_CLM_SEQ_NO  = SVC_CLP_CLM_SEQ_NO
  AND SVC_CAS_SVC_SEQ_NO      = SVC_SEQ_NO
  AND SVC_CAS_EXCL_RSN_CD     = 'C04'
LEFT OUTER JOIN
      &BAS.EVAL&BAS_TBL.EVAL B1
ON    B1.BAS_SCENARIO         = SVC_SCENARIO
  AND RTRIM(B1.BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
  AND B1.BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
  AND RTRIM(B1.BAS_CHG_IND)   = 'MOD'
  AND RTRIM(B1.BAS_RSN_CD)    = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD1)
LEFT OUTER JOIN
      &BAS.EVAL&BAS_TBL.EVAL B2
ON    B2.BAS_SCENARIO         = SVC_SCENARIO
  AND RTRIM(B2.BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
  AND B2.BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
  AND RTRIM(B2.BAS_CHG_IND)   = 'MOD'
  AND RTRIM(B2.BAS_RSN_CD)    = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD2)
LEFT OUTER JOIN
      &BAS.EVAL&BAS_TBL.EVAL B3
ON    B3.BAS_SCENARIO         = SVC_SCENARIO
  AND RTRIM(B3.BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
  AND B3.BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
  AND RTRIM(B3.BAS_CHG_IND)   = 'MOD'
  AND RTRIM(B3.BAS_RSN_CD)    = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD3)
  LEFT OUTER JOIN
      &BAS.EVAL&BAS_TBL.EVAL B4
ON    B4.BAS_SCENARIO         = SVC_SCENARIO
  AND RTRIM(B4.BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
  AND B4.BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
  AND RTRIM(B4.BAS_CHG_IND)   = 'MOD'
  AND RTRIM(B4.BAS_RSN_CD)    = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD4)
LEFT OUTER JOIN
      &BAS.EVAL&BAS_TBL.EVAL B5
ON    B5.BAS_SCENARIO         = SVC_SCENARIO
  AND RTRIM(B5.BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
  AND B5.BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
  AND RTRIM(B5.BAS_CHG_IND)   = 'MOD'
  AND RTRIM(B5.BAS_RSN_CD)    = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD5)
LEFT OUTER JOIN
      &BAS.EVAL&BAS_TBL.EVAL B6
ON    B6.BAS_SCENARIO         = SVC_SCENARIO
  AND RTRIM(B6.BAS_GRP_CD)    = RTRIM(SVC_CAS_CLM_ADJ_GRP_CD1)
  AND B6.BAS_CONTR_PYR        = SVC_CONTR_PAYOR_IND
  AND RTRIM(B6.BAS_CHG_IND)   = 'MOD'
  AND RTRIM(B6.BAS_RSN_CD)    = RTRIM(SVC_CAS_CLM_ADJ_RSN_CD6)
WHERE ST_EXCL_RSN_CD          = '   '
  AND (    RTRIM(B1.BAS_RSN_CD) IS NOT NULL
        OR RTRIM(B2.BAS_RSN_CD) IS NOT NULL
        OR RTRIM(B3.BAS_RSN_CD) IS NOT NULL
        OR RTRIM(B4.BAS_RSN_CD) IS NOT NULL
        OR RTRIM(B5.BAS_RSN_CD) IS NOT NULL
        OR RTRIM(B6.BAS_RSN_CD) IS NOT NULL )
ORDER BY 1,2,3,4,5,6,7
;
END
-RUN
