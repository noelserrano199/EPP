-STEP003
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP003';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL -
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC.EVAL&SVC_TBL.EVAL
WITH BASE AS (
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST INDEX(B EIX_CLARITY_TDL_TRAN_TXID) */ DISTINCT
      ERA_INV
					,ERA_STS
					,ERA_PMT
     ,SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
					,SVC_SEQ_NO
					,SVC_EPP_GEN_SEQ
					,B.TX_ID
					,B.CPT_CODE
					,B.ORIG_SERVICE_DATE
     ,B.ORIG_AMT
     ,B.MODIFIER_ONE
     ,B.MODIFIER_TWO
     ,B.MODIFIER_THREE
     ,B.MODIFIER_FOUR
     ,B.INSURANCE_AMOUNT
FROM
(
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
					 CAST(TO_NUMBER(TRIM( X.COLUMN_VALUE.EXTRACT( 'e/text()' ))) AS INTEGER)
					                                AS CLM_LN_TX_ID
     ,SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
					,SVC_SEQ_NO
					,SVC_EPP_GEN_SEQ
     ,ERA_INV
					,ERA_STS
					,ERA_PMT
					,TRANSACTION_LIST
FROM
(
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
					,SVC_SEQ_NO
					,SVC_EPP_GEN_SEQ
     ,CLP_CLM_SUBMTR_ID              AS ERA_INV
					,CLP_CLM_STATUS_CD              AS ERA_STS
					,TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0'))
					                                AS ERA_PMT
					,TRANSACTION_LIST
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL
ON    CLP_ISA_FILE_ID          = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO        = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO            = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD          = '   '
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL
ON    SVC_ISA_FILE_ID          = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO        = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO            = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO            = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO       = CLP_CLM_SEQ_NO
  AND SVC_EXCL_RSN_CD          = 'CPV'
INNER JOIN
      INV_CLM_LN_ADDL    A
ON    A.INVOICE_NUM     		   		= CLP_CLM_SUBMTR_ID
) T1
JOIN TABLE
      (XMLSEQUENCE(XMLTYPE('<e><e>'||REPLACE(T1.TRANSACTION_LIST,',','</e><e>')||'</e></e>').EXTRACT('e/e'))) X
ON    1                        = 1
ORDER BY 1
) T2
INNER JOIN
      CLARITY_TDL_TRAN   B
ON    B.TX_ID                  = T2.CLM_LN_TX_ID
  AND B.DETAIL_TYPE            = 20
		AND B.INSURANCE_AMOUNT       < 0
)
-*
SELECT
      B2.SVC_ISA_FILE_ID
     ,B2.SVC_GS_GRP_SEQ_NO
     ,B2.SVC_ST_SEQ_NO
     ,B2.SVC_LX_SEQ_NO
     ,B2.SVC_CLP_CLM_SEQ_NO
     ,( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ MAX(S4.SVC_SEQ_NO)
        FROM   &SVC.EVAL&SVC_TBL.EVAL S4
        WHERE  S4.SVC_ISA_FILE_ID    = B2.SVC_ISA_FILE_ID
          AND  S4.SVC_GS_GRP_SEQ_NO  = B2.SVC_GS_GRP_SEQ_NO
          AND  S4.SVC_ST_SEQ_NO      = B2.SVC_ST_SEQ_NO
          AND  S4.SVC_LX_SEQ_NO      = B2.SVC_LX_SEQ_NO
          AND  S4.SVC_CLP_CLM_SEQ_NO = B2.SVC_CLP_CLM_SEQ_NO )
     +RANK()
      OVER ( PARTITION BY B2.SVC_ISA_FILE_ID
                         ,B2.SVC_GS_GRP_SEQ_NO
                         ,B2.SVC_ST_SEQ_NO
                         ,B2.SVC_LX_SEQ_NO
                         ,B2.SVC_CLP_CLM_SEQ_NO
             ORDER BY     B2.SVC_ISA_FILE_ID
                         ,B2.SVC_GS_GRP_SEQ_NO
                         ,B2.SVC_ST_SEQ_NO
                         ,B2.SVC_LX_SEQ_NO
                         ,B2.SVC_CLP_CLM_SEQ_NO
                         ,B2.TX_ID ) AS SVC_SEQ_NO
     ,1                              AS SVC_EPP_GEN_SEQ
     ,':'                            AS SVC_COMP_SEPERATOR
     ,'HC'                           AS SVC_PROC1_SVC_ID_QLFR
     ,B2.CPT_CODE           AS SVC_PROC1_CD
     ,SUBSTR(COALESCE(B2.MODIFIER_ONE, '  '),1,2)
                                     AS SVC_PROC1_MODF1
     ,SUBSTR(COALESCE(B2.MODIFIER_TWO, '  '),1,2)
                                     AS SVC_PROC1_MODF2
     ,SUBSTR(COALESCE(B2.MODIFIER_THREE, '  '),1,2)
                                     AS SVC_PROC1_MODF3
     ,SUBSTR(COALESCE(B2.MODIFIER_FOUR, '  '),1,2)
                                     AS SVC_PROC1_MODF4
     ,CASE WHEN B2.ERA_STS = '22'
					           THEN 0-B2.ORIG_AMT
											ELSE B2.ORIG_AMT
						END                            AS SVC_ORGLI_PRVDR_PYMT
     ,B2.INSURANCE_AMOUNT            AS SVC_LI_PRVDR_PYMT
     ,' '                            AS SVC_NTL_BILNG_REV_CD
     ,' '                            AS SVC_UNTS_SVC_PD_CNT
     ,' '                            AS SVC_PROC2_SVC_ID_QLFR
     ,' '                            AS SVC_PROC2_CD
     ,' '                            AS SVC_PROC2_MODF1
     ,' '                            AS SVC_PROC2_MODF2
     ,' '                            AS SVC_PROC2_MODF3
     ,' '                            AS SVC_PROC2_MODF4
     ,' '                            AS SVC_PROC2_CD_DESC
     ,' '                            AS SVC_ORG_UNTS_SVC_CNT
     ,TO_CHAR(B2.TX_ID)              AS SVC_CLARITY_TX_ID
     ,'CPV'                          AS SVC_TX_ID_MATCH_TYPE
     ,'Y'                            AS SVC_CONTR_PAYOR_IND
     ,'N'                            AS SVC_DUP_PMT_IND
     ,B2.ERA_INV                     AS SVC_RVSD_INVC_NO
     ,0                              AS SVC_CURRENT_BALANCE
     ,0                              AS SVC_INS_AMOUNT_PAID
     ,0                              AS SVC_INS_AMOUNT_ADJ
     ,'N'                            AS SVC_CRG_IS_VOID_YN
     ,'N'                            AS SVC_PRIOR_DENIAL_YN
     ,TO_NUMBER(TO_CHAR(B2.ORIG_SERVICE_DATE,'YYYYMMDD'))
					                                AS SVC_MULTI_USE_AMOUNT
     ,'X'                            AS SVC_SCENARIO
     ,'CPV'                          AS SVC_INCL_RSN_CD
     ,'   '                          AS SVC_EXCL_RSN_CD
FROM
(
SELECT
      ERA_INV
					,ERA_STS
					,ERA_PMT
     ,SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
					,SUM(INSURANCE_AMOUNT)
FROM BASE B1
GROUP BY
      ERA_INV
					,ERA_STS
					,ERA_PMT
     ,SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
HAVING SUM(INSURANCE_AMOUNT) = ERA_PMT
) T1
INNER JOIN
      BASE B2
ON    B2.SVC_ISA_FILE_ID    = T1.SVC_ISA_FILE_ID
  AND B2.SVC_GS_GRP_SEQ_NO  = T1.SVC_GS_GRP_SEQ_NO
  AND B2.SVC_ST_SEQ_NO      = T1.SVC_ST_SEQ_NO
  AND B2.SVC_LX_SEQ_NO      = T1.SVC_LX_SEQ_NO
  AND B2.SVC_CLP_CLM_SEQ_NO = T1.SVC_CLP_CLM_SEQ_NO
;
END
-RUN
-*-----------------------------------------------------------------------------
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL COMPLETE RETCODE &RETCODE.EVAL
-TYPE -------------------------------------------------------------------------
-IF   &FOCERRNUM.EVAL NE 0 THEN GOTO ERRORS;
