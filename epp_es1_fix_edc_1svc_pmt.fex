-STEP004
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP004';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC_DTM.EVAL&SVC_DTM_TBL.EVAL
SELECT /*+ NOPARALLEL ORDERED */
      SVC_ISA_FILE_ID         AS SVC_DTM_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO       AS SVC_DTM_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO           AS SVC_DTM_ST_SEQ_NO
     ,SVC_LX_SEQ_NO           AS SVC_DTM_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO      AS SVC_DTM_CLP_CLM_SEQ_NO
     ,SVC_SEQ_NO              AS SVC_DTM_SVC_SEQ_NO
     ,1                       AS SVC_DTM_SEQ_NO
	 ,1                       AS SVC_DTM_EPP_GEN_SEQ
     ,'472'                   AS SVC_DTM_DTTM_QLFR
     ,TO_CHAR(ORIG_SERVICE_DATE, 'YYYYMMDD')
                              AS SVC_DTM_DATE
     ,' '                     AS SVC_DTM_TIME
     ,' '                     AS SVC_DTM_TIME_CD
     ,' '                     AS SVC_DTM_DTTM_PD_FMT_QLFR
     ,' '                     AS SVC_DTM_DTTM_PD
	 ,'ES1'                   AS SVC_DTM_INCL_RSN_CD
	 ,'   '                   AS SVC_DTM_EXCL_RSN_CD
FROM  &SVC.EVAL&SVC_TBL.EVAL
INNER JOIN
      CLARITY_TDL_TRAN
ON    TX_ID            = SVC_CLARITY_TX_ID
  AND DETAIL_TYPE      = 1
WHERE SVC_INCL_RSN_CD  = 'ES1'
  AND SVC_EXCL_RSN_CD  = '   '
ORDER BY 1,2,3,4,5,6,7
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
