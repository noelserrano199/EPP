SQL
INSERT INTO &CLP_DTM.EVAL&CLP_DTM_TBL.EVAL
SELECT /*+ ORDERED */ DISTINCT
      CLP_DTM_ISA_FILE_ID
     ,CLP_DTM_GS_GRP_SEQ_NO
     ,CLP_DTM_ST_SEQ_NO
     ,CLP_DTM_LX_SEQ_NO
     ,C1.CLP_CLM_SEQ_NO          AS CLP_DTM_CLP_CLM_SEQ_NO
     ,CLP_DTM_SEQ_NO             AS CLP_DTM_SEQ_NO
     ,CLP_DTM_EPP_GEN_SEQ+1      AS CLP_DTM_EPP_GEN_SEQ
     ,CLP_DTM_DTTM_QLFR
     ,CLP_DTM_DATE
     ,CLP_DTM_TIME
     ,CLP_DTM_TIME_CD
     ,CLP_DTM_DTTM_PD_FMT_QLFR
     ,CLP_DTM_DTTM_PD
     ,'MC2'                       AS CLP_DTM_INCL_RSN_CD
     ,'   '                       AS CLP_DTM_EXCL_RSN_CD
FROM  &CLP.EVAL&CLP_TBL.EVAL C1
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    RTRIM(C2.CLP_CLM_SUBMTR_ID) = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND RTRIM(C2.CLP_CLM_STATUS_CD) = RTRIM(C1.CLP_CLM_STATUS_CD) 
  AND C2.CLP_EXCL_RSN_CD          = 'MC2'
INNER JOIN
      &CLP_DTM.EVAL&CLP_DTM_TBL.EVAL
ON    CLP_DTM_ISA_FILE_ID         = C2.CLP_ISA_FILE_ID
  AND CLP_DTM_GS_GRP_SEQ_NO       = C2.CLP_GS_GRP_SEQ_NO
  AND CLP_DTM_ST_SEQ_NO           = C2.CLP_ST_SEQ_NO
  AND CLP_DTM_LX_SEQ_NO           = C2.CLP_LX_SEQ_NO
  AND CLP_DTM_CLP_CLM_SEQ_NO      = C2.CLP_CLM_SEQ_NO
  AND CLP_DTM_EXCL_RSN_CD         = '   '
WHERE C1.CLP_INCL_RSN_CD          = 'MC2'
  AND C1.CLP_EXCL_RSN_CD          = '   '
ORDER BY 1,2,3,4,5,6,7
;
TABLEF FILE SQLOUT
LIST *
END
