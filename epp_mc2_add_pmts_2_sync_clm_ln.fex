SQL
INSERT INTO &NM1.EVAL&NM1_TBL.EVAL
SELECT DISTINCT
      NM1_ISA_FILE_ID
     ,NM1_GS_GRP_SEQ_NO
     ,NM1_ST_SEQ_NO
     ,NM1_LX_SEQ_NO
     ,C1.CLP_CLM_SEQ_NO         AS NM1_CLP_CLM_SEQ_NO
     ,1                         AS NM1_EPP_GEN_SEQ
     ,NM1_ENTITY_ID_CD
     ,NM1_ENTITY_TYP_QLFR
     ,NM1_PT_LST_NM
     ,NM1_PT_FST_NM
     ,' '                       AS NM1_PT_MID_NM
     ,NM1_PT_NM_PRFX
     ,NM1_PT_NM_SFX
     ,NM1_INS_ID_CD_QLFR
     ,NM1_INS_ID_CD
     ,NM1_ENTITY_REL_CD
     ,NM1_ENTITY_ID_CD2
     ,NM1_ORG_NM
     ,'MC2'                     AS NM1_INCL_RSN_CD
     ,'   '                     AS NM1_EXCL_RSN_CD
FROM  &CLP.EVAL&CLP_TBL.EVAL C1
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    RTRIM(C2.CLP_CLM_SUBMTR_ID) = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND RTRIM(C2.CLP_CLM_STATUS_CD) = RTRIM(C1.CLP_CLM_STATUS_CD) 
  AND C2.CLP_EXCL_RSN_CD          = 'MC2'
INNER JOIN
      &NM1.EVAL&NM1_TBL.EVAL
ON    NM1_ISA_FILE_ID             = C2.CLP_ISA_FILE_ID
  AND NM1_GS_GRP_SEQ_NO           = C2.CLP_GS_GRP_SEQ_NO
  AND NM1_ST_SEQ_NO               = C2.CLP_ST_SEQ_NO
  AND NM1_LX_SEQ_NO               = C2.CLP_LX_SEQ_NO
  AND NM1_CLP_CLM_SEQ_NO          = C2.CLP_CLM_SEQ_NO
  AND NM1_EXCL_RSN_CD             = '   '
WHERE C1.CLP_INCL_RSN_CD          = 'MC2'
  AND C1.CLP_EXCL_RSN_CD          = '   '
ORDER BY  2,3,4,5,6,7,8
;
END
-RUN
-*-----------------------------------------------------------------------------
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL COMPLETE RETCODE&RETCODE.EVAL
-TYPE -------------------------------------------------------------------------
-IF   &FOCERRNUM.EVAL NE 0 THEN GOTO ERRORS;
-*-----------------------------------------------------------------------------
-STEP010
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP010';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC.EVAL&SVC_TBL.EVAL UT
SELECT
      C2.CLP_ISA_FILE_ID   AS SVC_ISA_FILE_ID
     ,C2.CLP_GS_GRP_SEQ_NO AS SVC_GS_GRP_SEQ_NO
     ,C2.CLP_ST_SEQ_NO     AS SVC_ST_SEQ_NO
     ,C2.CLP_LX_SEQ_NO     AS SVC_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO    AS SVC_CLP_CLM_SEQ_NO
     ,DENSE_RANK()
      OVER ( PARTITION BY C2.CLP_ISA_FILE_ID
                         ,C2.CLP_GS_GRP_SEQ_NO
                         ,C2.CLP_ST_SEQ_NO
                         ,C2.CLP_LX_SEQ_NO
                         ,C2.CLP_CLM_SEQ_NO
             ORDER BY     C2.CLP_ISA_FILE_ID
                         ,C2.CLP_GS_GRP_SEQ_NO
                         ,C2.CLP_ST_SEQ_NO
                         ,C2.CLP_LX_SEQ_NO
                         ,C2.CLP_CLM_SEQ_NO
                         ,ERA_CPT )
                           AS SVC_SEQ_NO
     ,1                    AS SVC_EPP_GEN_SEQ
     ,':'                  AS SVC_COMP_SEPERATOR
     ,'HC'                 AS SVC_PROC1_SVC_ID_QLFR
     ,ERA_CPT              AS SVC_PROC1_CD
     ,' '                  AS SVC_PROC1_MODF1
     ,' '                  AS SVC_PROC1_MODF2
     ,' '                  AS SVC_PROC1_MODF3
     ,' '                  AS SVC_PROC1_MODF4
     ,ERA_CRG              AS SVC_ORGLI_PRVDR_PYMT
     ,'0'                  AS SVC_LI_PRVDR_PYMT
     ,' '                  AS SVC_NTL_BILNG_REV_CD
     ,' '                  AS SVC_UNTS_SVC_PD_CNT
     ,' '                  AS SVC_PROC2_SVC_ID_QLFR
     ,' '                  AS SVC_PROC2_CD
     ,' '                  AS SVC_PROC2_MODF1
     ,' '                  AS SVC_PROC2_MODF2
     ,' '                  AS SVC_PROC2_MODF3
     ,' '                  AS SVC_PROC2_MODF4
     ,' '                  AS SVC_PROC2_CD_DESC
     ,' '                  AS SVC_ORG_UNTS_SVC_CNT
     ,'0'                  AS SVC_CLARITY_TX_ID
     ,'MC2'                AS SVC_TX_ID_MATCH_TYPE
     ,'Y'                  AS SVC_CONTR_PAYOR_IND
     ,'N'                  AS SVC_DUP_PMT_IND
     ,ERA_INV              AS SVC_RVSD_INVC_NO
     ,TO_NUMBER(TO_CHAR(ERA_DOS,'YYYYMMDD'))
                           AS SVC_CURRENT_BALANCE
     ,0                    AS SVC_INS_AMOUNT_PAID
     ,0                    AS SVC_INS_AMOUNT_ADJ
     ,'N'                  AS SVC_CRG_IS_VOID_YN
     ,'N'                  AS SVC_PRIOR_DENIAL_YN
     ,0                    AS SVC_MULTI_USE_AMOUNT
     ,'X'                  AS SVC_SCENARIO
     ,'MC2'                AS SVC_INCL_RSN_CD
     ,'   '                AS SVC_EXCL_RSN_CD
FROM
(
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      RTRIM(CLP_CLM_SUBMTR_ID)           AS ERA_INV
     ,RTRIM(SVC_PROC1_CD)                AS ERA_CPT
     ,TO_DATE(SVC_DTM_DATE,'YYYYMMDD')   AS ERA_DOS
     ,TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0'))
                                         AS ERA_CRG
     ,RTRIM(CLP_CLM_STATUS_CD)           AS ERA_STS
     ,CLP_ISA_FILE_ID
     ,CLP_GS_GRP_SEQ_NO
     ,CLP_ST_SEQ_NO
     ,CLP_LX_SEQ_NO
     ,CLP_CLM_SEQ_NO
     ,CLP_EPP_GEN_SEQ
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C1
ON    CLP_ISA_FILE_ID         = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO       = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO           = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD         = 'MC2'
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL S1
ON    SVC_ISA_FILE_ID         = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO       = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO           = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO           = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO      = CLP_CLM_SEQ_NO
  AND SVC_EXCL_RSN_CD         = '   '
  AND TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0')) > 0
INNER JOIN
      &SVC_DTM.EVAL&SVC_DTM_TBL.EVAL D1
ON    SVC_DTM_ISA_FILE_ID     = SVC_ISA_FILE_ID
  AND SVC_DTM_GS_GRP_SEQ_NO   = SVC_GS_GRP_SEQ_NO
  AND SVC_DTM_ST_SEQ_NO       = SVC_ST_SEQ_NO
  AND SVC_DTM_LX_SEQ_NO       = SVC_LX_SEQ_NO
  AND SVC_DTM_CLP_CLM_SEQ_NO  = SVC_CLP_CLM_SEQ_NO
  AND SVC_DTM_SVC_SEQ_NO      = SVC_SEQ_NO
  AND SVC_DTM_DTTM_QLFR       IN( '472','150' )
  AND SVC_DTM_EXCL_RSN_CD     = '   '
WHERE ST_EXCL_RSN_CD          = '   '
GROUP BY
      RTRIM(CLP_CLM_SUBMTR_ID)
     ,RTRIM(SVC_PROC1_CD)
     ,TO_DATE(SVC_DTM_DATE,'YYYYMMDD')
     ,TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0'))
     ,RTRIM(CLP_CLM_STATUS_CD)
     ,CLP_ISA_FILE_ID
     ,CLP_GS_GRP_SEQ_NO
     ,CLP_ST_SEQ_NO
     ,CLP_LX_SEQ_NO
     ,CLP_CLM_SEQ_NO
     ,CLP_EPP_GEN_SEQ
) T1
INNER JOIN
      INV_CLM_LN_ADDL   A
ON    A.INVOICE_NUM           = ERA_INV
  AND A.PROC_OR_REV_CODE      = ERA_CPT
  AND A.FROM_SVC_DATE         = ERA_DOS
  AND A.CHARGE_AMOUNT         = ERA_CRG
  AND 0                       = INSTR(A.TRANSACTION_LIST,',')
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    C2.CLP_ISA_FILE_ID      = T1.CLP_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO    = T1.CLP_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO        = T1.CLP_ST_SEQ_NO
  AND C2.CLP_LX_SEQ_NO        = T1.CLP_LX_SEQ_NO
  AND C2.CLP_INCL_RSN_CD      = 'MC2'
  AND RTRIM(C2.CLP_CLM_SUBMTR_ID) = ERA_INV
WHERE NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  &SVC.EVAL&SVC_TBL.EVAL         S3
           ,&SVC_DTM.EVAL&SVC_DTM_TBL.EVAL D3
      WHERE S3.SVC_ISA_FILE_ID          = T1.CLP_ISA_FILE_ID
        AND S3.SVC_GS_GRP_SEQ_NO        = T1.CLP_GS_GRP_SEQ_NO
        AND S3.SVC_ST_SEQ_NO            = T1.CLP_ST_SEQ_NO
        AND S3.SVC_LX_SEQ_NO            = T1.CLP_LX_SEQ_NO
        AND S3.SVC_CLP_CLM_SEQ_NO       = T1.CLP_CLM_SEQ_NO
        AND RTRIM(S3.SVC_PROC1_CD)      = ERA_CPT
        AND TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0')) = 0
        AND S3.SVC_EXCL_RSN_CD          = '   '
        AND D3.SVC_DTM_ISA_FILE_ID      = S3.SVC_ISA_FILE_ID
        AND D3.SVC_DTM_GS_GRP_SEQ_NO    = S3.SVC_GS_GRP_SEQ_NO
        AND D3.SVC_DTM_ST_SEQ_NO        = S3.SVC_ST_SEQ_NO
        AND D3.SVC_DTM_LX_SEQ_NO        = S3.SVC_LX_SEQ_NO
        AND D3.SVC_DTM_CLP_CLM_SEQ_NO   = S3.SVC_CLP_CLM_SEQ_NO
        AND D3.SVC_DTM_SVC_SEQ_NO       = S3.SVC_SEQ_NO
        AND D3.SVC_DTM_DTTM_QLFR        IN( '472','150' )
        AND D3.SVC_DTM_EXCL_RSN_CD      = '   '
        AND TO_DATE(D3.SVC_DTM_DATE,'YYYYMMDD') = ERA_DOS )
  AND EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  INV_CLM_LN_ADDL   X
           ,ARPB_TRANSACTIONS Y
      WHERE X.INVOICE_NUM      = ERA_INV
        AND X.PROC_OR_REV_CODE = ERA_CPT
        AND X.FROM_SVC_DATE    = ERA_DOS
        AND X.CHARGE_AMOUNT    = ERA_CRG
        AND 0                  = INSTR(X.TRANSACTION_LIST,',')
        AND Y.TX_ID            = TO_NUMBER(X.TRANSACTION_LIST)
        AND Y.OUTSTANDING_AMT <= 0 )
  AND EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  INV_CLM_LN_ADDL   X
           ,ARPB_TRANSACTIONS Y
      WHERE X.INVOICE_NUM      = ERA_INV
        AND X.PROC_OR_REV_CODE = ERA_CPT
        AND X.FROM_SVC_DATE    = ERA_DOS
        AND X.CHARGE_AMOUNT    = ERA_CRG
        AND 0                  = INSTR(X.TRANSACTION_LIST,',')
        AND Y.TX_ID            = TO_NUMBER(X.TRANSACTION_LIST)
        AND Y.OUTSTANDING_AMT  > 0 )
  AND NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  &CLP.EVAL&CLP_TBL.EVAL C3
           ,&SVC.EVAL&SVC_TBL.EVAL S3
           ,&SVC_DTM.EVAL&SVC_DTM_TBL.EVAL D3
      WHERE SUBSTR(RTRIM(C3.CLP_CLM_SUBMTR_ID),1,10)
               = SUBSTR(ERA_INV,1,10)
        AND C3.CLP_CLM_STATUS_CD        = '22'
        AND C3.CLP_EXCL_RSN_CD          = '   '
        AND S3.SVC_ISA_FILE_ID          = C3.CLP_ISA_FILE_ID
        AND S3.SVC_GS_GRP_SEQ_NO        = C3.CLP_GS_GRP_SEQ_NO
        AND S3.SVC_ST_SEQ_NO            = C3.CLP_ST_SEQ_NO
        AND S3.SVC_LX_SEQ_NO            = C3.CLP_LX_SEQ_NO
        AND S3.SVC_CLP_CLM_SEQ_NO       = C3.CLP_CLM_SEQ_NO
        AND RTRIM(S3.SVC_PROC1_CD)      = ERA_CPT
        AND D3.SVC_DTM_ISA_FILE_ID      = S3.SVC_ISA_FILE_ID
        AND D3.SVC_DTM_GS_GRP_SEQ_NO    = S3.SVC_GS_GRP_SEQ_NO
        AND D3.SVC_DTM_ST_SEQ_NO        = S3.SVC_ST_SEQ_NO
        AND D3.SVC_DTM_LX_SEQ_NO        = S3.SVC_LX_SEQ_NO
        AND D3.SVC_DTM_CLP_CLM_SEQ_NO   = S3.SVC_CLP_CLM_SEQ_NO
        AND D3.SVC_DTM_SVC_SEQ_NO       = S3.SVC_SEQ_NO
        AND D3.SVC_DTM_DTTM_QLFR        IN('472','150')
        AND D3.SVC_DTM_EXCL_RSN_CD      = '   '
        AND TO_DATE(D3.SVC_DTM_DATE, 'YYYYMMDD')
                                        = ERA_DOS)
GROUP BY
      C2.CLP_ISA_FILE_ID
     ,C2.CLP_GS_GRP_SEQ_NO
     ,C2.CLP_ST_SEQ_NO
     ,C2.CLP_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO
     ,ERA_INV
     ,ERA_CPT
     ,ERA_DOS
     ,ERA_CRG
     ,ERA_STS
ORDER BY 1,2,3,4
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
-*-----------------------------------------------------------------------------
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL COMPLETE RETCODE&RETCODE.EVAL
-TYPE -------------------------------------------------------------------------
-IF   &FOCERRNUM.EVAL NE 0 THEN GOTO ERRORS;
-*-----------------------------------------------------------------------------
-STEP011
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP011';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC.EVAL&SVC_TBL.EVAL UT
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      T2.CLP_ISA_FILE_ID   AS SVC_ISA_FILE_ID
     ,T2.CLP_GS_GRP_SEQ_NO AS SVC_GS_GRP_SEQ_NO
     ,T2.CLP_ST_SEQ_NO     AS SVC_ST_SEQ_NO
     ,T2.CLP_LX_SEQ_NO     AS SVC_LX_SEQ_NO
     ,T2.NEW_CLM_SEQ_NO    AS SVC_CLP_CLM_SEQ_NO
     ,( SELECT MAX(SVC_SEQ_NO)
         FROM   &SVC.EVAL&SVC_TBL.EVAL S4
         WHERE  S4.SVC_ISA_FILE_ID    = T2.CLP_ISA_FILE_ID
           AND  S4.SVC_GS_GRP_SEQ_NO  = T2.CLP_GS_GRP_SEQ_NO
           AND  S4.SVC_ST_SEQ_NO      = T2.CLP_ST_SEQ_NO
           AND  S4.SVC_LX_SEQ_NO      = T2.CLP_LX_SEQ_NO
           AND  S4.SVC_CLP_CLM_SEQ_NO = T2.NEW_CLM_SEQ_NO )
      +DENSE_RANK()
       OVER ( PARTITION BY T2.CLP_ISA_FILE_ID
                          ,T2.CLP_GS_GRP_SEQ_NO
                          ,T2.CLP_ST_SEQ_NO
                          ,T2.CLP_LX_SEQ_NO
              ORDER BY     T2.CLP_ISA_FILE_ID
                          ,T2.CLP_GS_GRP_SEQ_NO
                          ,T2.CLP_ST_SEQ_NO
                          ,T2.CLP_LX_SEQ_NO
                          ,T2.OLD_CLM_SEQ_NO
                          ,SVC_SEQ_NO )
                           AS SVC_SEQ_NO
     ,1                    AS SVC_EPP_GEN_SEQ
     ,':'                  AS SVC_COMP_SEPERATOR
     ,'HC'                 AS SVC_PROC1_SVC_ID_QLFR
     ,SVC_PROC1_CD
     ,SVC_PROC1_MODF1
     ,SVC_PROC1_MODF2
     ,SVC_PROC1_MODF3
     ,SVC_PROC1_MODF4
     ,SVC_ORGLI_PRVDR_PYMT
     ,SVC_LI_PRVDR_PYMT
     ,SVC_NTL_BILNG_REV_CD
     ,SVC_UNTS_SVC_PD_CNT
     ,SVC_PROC2_SVC_ID_QLFR
     ,SVC_PROC2_CD
     ,SVC_PROC2_MODF1
     ,SVC_PROC2_MODF2
     ,SVC_PROC2_MODF3
     ,SVC_PROC2_MODF4
     ,SVC_PROC2_CD_DESC
     ,SVC_ORG_UNTS_SVC_CNT
     ,SVC_CLARITY_TX_ID
     ,SVC_TX_ID_MATCH_TYPE
     ,SVC_CONTR_PAYOR_IND
     ,'N'                  AS SVC_DUP_PMT_IND
     ,ERA_INV              AS SVC_RVSD_INVC_NO
     ,TO_NUMBER(TO_CHAR(ERA_DOS,'YYYYMMDD'))
                           AS SVC_CURRENT_BALANCE
     ,OLD_CLM_SEQ_NO       AS SVC_INS_AMOUNT_PAID
     ,SVC_SEQ_NO           AS SVC_INS_AMOUNT_ADJ
     ,SVC_CRG_IS_VOID_YN
     ,SVC_PRIOR_DENIAL_YN
     ,SVC_MULTI_USE_AMOUNT
     ,SVC_SCENARIO
     ,'MC2'                AS SVC_INCL_RSN_CD
     ,'   '                AS SVC_EXCL_RSN_CD
FROM
(
SELECT
      C2.CLP_ISA_FILE_ID
     ,C2.CLP_GS_GRP_SEQ_NO
     ,C2.CLP_ST_SEQ_NO
     ,C2.CLP_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO   AS NEW_CLM_SEQ_NO
     ,T1.CLP_CLM_SEQ_NO   AS OLD_CLM_SEQ_NO
     ,ERA_INV
     ,ERA_CPT
     ,ERA_DOS
     ,ERA_CRG
     ,ERA_STS
FROM
(
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      RTRIM(CLP_CLM_SUBMTR_ID)           AS ERA_INV
     ,RTRIM(SVC_PROC1_CD)                AS ERA_CPT
     ,TO_DATE(SVC_DTM_DATE,'YYYYMMDD')   AS ERA_DOS
     ,TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0'))
                                         AS ERA_CRG
     ,RTRIM(CLP_CLM_STATUS_CD)           AS ERA_STS
     ,CLP_ISA_FILE_ID
     ,CLP_GS_GRP_SEQ_NO
     ,CLP_ST_SEQ_NO
     ,CLP_LX_SEQ_NO
     ,CLP_CLM_SEQ_NO
     ,CLP_EPP_GEN_SEQ
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C1
ON    CLP_ISA_FILE_ID         = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO       = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO           = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD         = 'MC2'
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL S1
ON    SVC_ISA_FILE_ID         = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO       = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO           = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO           = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO      = CLP_CLM_SEQ_NO
  AND SVC_EXCL_RSN_CD         = '   '
  AND TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0')) > 0
INNER JOIN
      &SVC_DTM.EVAL&SVC_DTM_TBL.EVAL D1
ON    SVC_DTM_ISA_FILE_ID     = SVC_ISA_FILE_ID
  AND SVC_DTM_GS_GRP_SEQ_NO   = SVC_GS_GRP_SEQ_NO
  AND SVC_DTM_ST_SEQ_NO       = SVC_ST_SEQ_NO
  AND SVC_DTM_LX_SEQ_NO       = SVC_LX_SEQ_NO
  AND SVC_DTM_CLP_CLM_SEQ_NO  = SVC_CLP_CLM_SEQ_NO
  AND SVC_DTM_SVC_SEQ_NO      = SVC_SEQ_NO
  AND SVC_DTM_DTTM_QLFR       IN( '472','150' )
  AND SVC_DTM_EXCL_RSN_CD     = '   '
WHERE ST_EXCL_RSN_CD          = '   '
GROUP BY
      RTRIM(CLP_CLM_SUBMTR_ID)
     ,RTRIM(SVC_PROC1_CD)
     ,TO_DATE(SVC_DTM_DATE,'YYYYMMDD')
     ,TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0'))
     ,RTRIM(CLP_CLM_STATUS_CD)
     ,CLP_ISA_FILE_ID
     ,CLP_GS_GRP_SEQ_NO
     ,CLP_ST_SEQ_NO
     ,CLP_LX_SEQ_NO
     ,CLP_CLM_SEQ_NO
     ,CLP_EPP_GEN_SEQ
) T1
INNER JOIN
      INV_CLM_LN_ADDL   A
ON    A.INVOICE_NUM           = ERA_INV
  AND A.PROC_OR_REV_CODE      = ERA_CPT
  AND A.FROM_SVC_DATE         = ERA_DOS
  AND A.CHARGE_AMOUNT         = ERA_CRG
  AND 0                       = INSTR(A.TRANSACTION_LIST,',')
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    C2.CLP_ISA_FILE_ID      = T1.CLP_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO    = T1.CLP_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO        = T1.CLP_ST_SEQ_NO
  AND C2.CLP_LX_SEQ_NO        = T1.CLP_LX_SEQ_NO
  AND C2.CLP_INCL_RSN_CD      = 'MC2'
  AND RTRIM(C2.CLP_CLM_SUBMTR_ID) = ERA_INV
WHERE NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  &SVC.EVAL&SVC_TBL.EVAL         S3
           ,&SVC_DTM.EVAL&SVC_DTM_TBL.EVAL D3
      WHERE S3.SVC_ISA_FILE_ID          = T1.CLP_ISA_FILE_ID
        AND S3.SVC_GS_GRP_SEQ_NO        = T1.CLP_GS_GRP_SEQ_NO
        AND S3.SVC_ST_SEQ_NO            = T1.CLP_ST_SEQ_NO
        AND S3.SVC_LX_SEQ_NO            = T1.CLP_LX_SEQ_NO
        AND S3.SVC_CLP_CLM_SEQ_NO       = T1.CLP_CLM_SEQ_NO
        AND RTRIM(S3.SVC_PROC1_CD)      = ERA_CPT
        AND TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0')) = 0
        AND S3.SVC_EXCL_RSN_CD          = '   '
        AND D3.SVC_DTM_ISA_FILE_ID      = S3.SVC_ISA_FILE_ID
        AND D3.SVC_DTM_GS_GRP_SEQ_NO    = S3.SVC_GS_GRP_SEQ_NO
        AND D3.SVC_DTM_ST_SEQ_NO        = S3.SVC_ST_SEQ_NO
        AND D3.SVC_DTM_LX_SEQ_NO        = S3.SVC_LX_SEQ_NO
        AND D3.SVC_DTM_CLP_CLM_SEQ_NO   = S3.SVC_CLP_CLM_SEQ_NO
        AND D3.SVC_DTM_SVC_SEQ_NO       = S3.SVC_SEQ_NO
        AND D3.SVC_DTM_DTTM_QLFR        IN( '472','150' )
        AND D3.SVC_DTM_EXCL_RSN_CD      = '   '
        AND TO_DATE(D3.SVC_DTM_DATE,'YYYYMMDD') = ERA_DOS )
  AND EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  INV_CLM_LN_ADDL   X
           ,ARPB_TRANSACTIONS Y
      WHERE X.INVOICE_NUM      = ERA_INV
        AND X.PROC_OR_REV_CODE = ERA_CPT
        AND X.FROM_SVC_DATE    = ERA_DOS
        AND X.CHARGE_AMOUNT    = ERA_CRG
        AND 0                  = INSTR(X.TRANSACTION_LIST,',')
        AND Y.TX_ID            = TO_NUMBER(X.TRANSACTION_LIST)
        AND Y.OUTSTANDING_AMT <= 0 )
  AND EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  INV_CLM_LN_ADDL   X
           ,ARPB_TRANSACTIONS Y
      WHERE X.INVOICE_NUM      = ERA_INV
        AND X.PROC_OR_REV_CODE = ERA_CPT
        AND X.FROM_SVC_DATE    = ERA_DOS
        AND X.CHARGE_AMOUNT    = ERA_CRG
        AND 0                  = INSTR(X.TRANSACTION_LIST,',')
        AND Y.TX_ID            = TO_NUMBER(X.TRANSACTION_LIST)
        AND Y.OUTSTANDING_AMT  > 0 )
  AND NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  &CLP.EVAL&CLP_TBL.EVAL C3
           ,&SVC.EVAL&SVC_TBL.EVAL S3
           ,&SVC_DTM.EVAL&SVC_DTM_TBL.EVAL D3
      WHERE SUBSTR(RTRIM(C3.CLP_CLM_SUBMTR_ID),1,10)
               = SUBSTR(ERA_INV,1,10)
        AND C3.CLP_CLM_STATUS_CD        = '22'
        AND C3.CLP_EXCL_RSN_CD          = '   '
        AND S3.SVC_ISA_FILE_ID          = C3.CLP_ISA_FILE_ID
        AND S3.SVC_GS_GRP_SEQ_NO        = C3.CLP_GS_GRP_SEQ_NO
        AND S3.SVC_ST_SEQ_NO            = C3.CLP_ST_SEQ_NO
        AND S3.SVC_LX_SEQ_NO            = C3.CLP_LX_SEQ_NO
        AND S3.SVC_CLP_CLM_SEQ_NO       = C3.CLP_CLM_SEQ_NO
        AND RTRIM(S3.SVC_PROC1_CD)      = ERA_CPT
        AND D3.SVC_DTM_ISA_FILE_ID      = S3.SVC_ISA_FILE_ID
        AND D3.SVC_DTM_GS_GRP_SEQ_NO    = S3.SVC_GS_GRP_SEQ_NO
        AND D3.SVC_DTM_ST_SEQ_NO        = S3.SVC_ST_SEQ_NO
        AND D3.SVC_DTM_LX_SEQ_NO        = S3.SVC_LX_SEQ_NO
        AND D3.SVC_DTM_CLP_CLM_SEQ_NO   = S3.SVC_CLP_CLM_SEQ_NO
        AND D3.SVC_DTM_SVC_SEQ_NO       = S3.SVC_SEQ_NO
        AND D3.SVC_DTM_DTTM_QLFR        IN('472','150')
        AND D3.SVC_DTM_EXCL_RSN_CD      = '   '
        AND TO_DATE(D3.SVC_DTM_DATE, 'YYYYMMDD')
                                        = ERA_DOS)
GROUP BY
      C2.CLP_ISA_FILE_ID
     ,C2.CLP_GS_GRP_SEQ_NO
     ,C2.CLP_ST_SEQ_NO
     ,C2.CLP_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO
     ,T1.CLP_CLM_SEQ_NO
     ,ERA_INV
     ,ERA_CPT
     ,ERA_DOS
     ,ERA_CRG
     ,ERA_STS
ORDER BY 1,2,3,4
) T2
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL S2
ON    S2.SVC_ISA_FILE_ID      = T2.CLP_ISA_FILE_ID
  AND S2.SVC_GS_GRP_SEQ_NO    = T2.CLP_GS_GRP_SEQ_NO
  AND S2.SVC_ST_SEQ_NO        = T2.CLP_ST_SEQ_NO
  AND S2.SVC_LX_SEQ_NO        = T2.CLP_LX_SEQ_NO
  AND S2.SVC_CLP_CLM_SEQ_NO   = T2.OLD_CLM_SEQ_NO
  AND RTRIM(S2.SVC_PROC1_CD)  = ERA_CPT
  AND S2.SVC_EXCL_RSN_CD      = '   '
  AND TO_NUMBER(COALESCE(RTRIM(S2.SVC_ORGLI_PRVDR_PYMT),'0')) = ERA_CRG
;
TABLEF FILE SQLOUT
LIST *
END
