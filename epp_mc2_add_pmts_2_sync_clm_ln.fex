-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &CLP.EVAL&CLP_TBL.EVAL
SELECT DISTINCT
      CLP_ISA_FILE_ID          AS CLP_ISA_FILE_ID
     ,CLP_GS_GRP_SEQ_NO        AS CLP_GS_GRP_SEQ_NO
     ,CLP_ST_SEQ_NO            AS CLP_ST_SEQ_NO
     ,CLP_LX_SEQ_NO            AS CLP_LX_SEQ_NO
     ,( SELECT MAX(CLP_CLM_SEQ_NO)
         FROM   &CLP.EVAL&CLP_TBL.EVAL C2
         WHERE  C2.CLP_ISA_FILE_ID    = C1.CLP_ISA_FILE_ID
           AND  C2.CLP_GS_GRP_SEQ_NO  = C1.CLP_GS_GRP_SEQ_NO
           AND  C2.CLP_ST_SEQ_NO      = C1.CLP_ST_SEQ_NO
           AND  C2.CLP_LX_SEQ_NO      = C1.CLP_LX_SEQ_NO )
      +DENSE_RANK()
       OVER ( PARTITION BY CLP_ISA_FILE_ID
                          ,CLP_GS_GRP_SEQ_NO
                          ,CLP_ST_SEQ_NO
                          ,CLP_LX_SEQ_NO
              ORDER BY     CLP_ISA_FILE_ID
                          ,CLP_GS_GRP_SEQ_NO
                          ,CLP_ST_SEQ_NO
                          ,CLP_LX_SEQ_NO
                          ,CLP_CLM_SUBMTR_ID )
                               AS CLP_CLM_SEQ_NO
     ,1                        AS CLP_EPP_GEN_SEQ
     ,CLP_CLM_SUBMTR_ID
     ,CLP_CLM_STATUS_CD
     ,0                        AS CLP_TOT_CLM_CRG_AMT
     ,0                        AS CLP_CLM_PYMT_AMT
     ,' '                      AS CLP_PT_RESP_AMT
     ,MIN(CLP_CLM_FIL_ID_CD)   AS CLP_CLM_FIL_ID_CD
     ,MIN(CLP_PYR_CLM_CNTL_NO) AS CLP_PYR_CLM_CNTL_NO
     ,MIN(CLP_FCLTY_CD_VALU)   AS CLP_FCLTY_CD_VALU
     ,MIN(CLP_CLM_FREQ_TYP_CD) AS CLP_CLM_FREQ_TYP_CD
     ,MIN(CLP_PT_STS_CD)       AS CLP_PT_STS_CD
     ,MIN(CLP_DRG_CD)          AS CLP_DRG_CD
     ,MIN(CLP_QTY)             AS CLP_QTY
     ,MIN(CLP_PCT)             AS CLP_PCT
     ,MIN(CLP_YN_COND_RESP_CD) AS CLP_YN_COND_RESP_CD
     ,'MC2'                    AS CLP_CL_TX_MATCH_TYPE
     ,' '                      AS CLP_RVSD_INVC_NO
     ,' '                      AS CLP_INVC_FIN_CLASS
     ,0                        AS CLP_INVC_CRG_COUNT
     ,0                        AS CLP_MATCHED_CRG_CNT
     ,0                        AS CLP_ORIG_PRICE
     ,0                        AS CLP_CURRENT_BALANCE
     ,0                        AS CLP_MULTI_USE_AMOUNT
     ,' '                      AS CLP_IS_TAKEBACK_YN
     ,'MC2'                    AS CLP_INCL_RSN_CD
     ,'   '                    AS CLP_EXCL_RSN_CD
FROM  &CLP.EVAL&CLP_TBL.EVAL C1
WHERE CLP_EXCL_RSN_CD = 'MC2'
GROUP BY
      CLP_ISA_FILE_ID
     ,CLP_GS_GRP_SEQ_NO
     ,CLP_ST_SEQ_NO
     ,CLP_LX_SEQ_NO
     ,CLP_CLM_SUBMTR_ID
     ,CLP_CLM_STATUS_CD
ORDER BY 1,2,3,4,5,6,7
;
END
-RUN
