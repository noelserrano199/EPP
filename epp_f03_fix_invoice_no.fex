-STEP004
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP004';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-TYPE *** Write a new SVC which will be connected to a new CLP - different invoice
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC.EVAL&SVC_TBL.EVAL
SELECT DISTINCT
      SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO            AS SVC_CLP_CLM_SEQ_NO
     ,SVC_SEQ_NO                   AS SVC_SEQ_NO
     ,1                            AS SVC_EPP_GEN_SEQ
     ,SVC_COMP_SEPERATOR
     ,SVC_PROC1_SVC_ID_QLFR
     ,SUBSTR(SVC_RVSD_INVC_NO,INSTR(SVC_RVSD_INVC_NO,'|',1,3)+1,LENGTH(SVC_RVSD_INVC_NO)-INSTR(SVC_RVSD_INVC_NO,'|',1,3))
                                   AS SVC_PROC1_CD
     ,SUBSTR(COALESCE(A.MODIFIER_ONE,' '),1,2)
                                   AS SVC_PROC1_MODF1
     ,SUBSTR(COALESCE(A.MODIFIER_TWO,' '),1,2)
                                   AS SVC_PROC1_MODF2
     ,SUBSTR(COALESCE(A.MODIFIER_THREE,' '),1,2)
                                   AS SVC_PROC1_MODF3
     ,SUBSTR(COALESCE(A.MODIFIER_FOUR,' '),1,2)
                                   AS SVC_PROC1_MODF4
     ,SVC_MULTI_USE_AMOUNT         AS SVC_ORGLI_PRVDR_PYMT
     ,SVC_LI_PRVDR_PYMT
     ,SVC_NTL_BILNG_REV_CD
     ,SVC_UNTS_SVC_PD_CNT
     ,' '                          AS SVC_PROC2_SVC_ID_QLFR
     ,' '                          AS SVC_PROC2_CD
     ,' '                          AS SVC_PROC2_MODF1
     ,' '                          AS SVC_PROC2_MODF2
     ,' '                          AS SVC_PROC2_MODF3
     ,' '                          AS SVC_PROC2_MODF4
     ,' '                          AS SVC_PROC2_CD_DESC
     ,SVC_ORG_UNTS_SVC_CNT
     ,SVC_CLARITY_TX_ID
     ,SUBSTR(SVC_RVSD_INVC_NO,INSTR(SVC_RVSD_INVC_NO,'|',1,1)+1,INSTR(SVC_RVSD_INVC_NO,'|',1,2)-INSTR(SVC_RVSD_INVC_NO,'|',1,1)-1)
                                AS SVC_TX_ID_MATCH_TYPE
     ,SVC_CONTR_PAYOR_IND
     ,SVC_DUP_PMT_IND
     ,SUBSTR(SVC_RVSD_INVC_NO,1,INSTR(SVC_RVSD_INVC_NO,'|')-1)
                                AS SVC_RVSD_INVC_NO
     ,B.OUTSTANDING_AMT         AS SVC_CURRENT_BALANCE
     ,SVC_INS_AMOUNT_PAID
     ,SVC_INS_AMOUNT_ADJ
     ,SVC_CRG_IS_VOID_YN
     ,SVC_PRIOR_DENIAL_YN
     ,SVC_CLP_CLM_SEQ_NO        AS SVC_MULTI_USE_AMOUNT
     ,SVC_SCENARIO
     ,'F03'                     AS SVC_INCL_RSN_CD
     ,'   '                     AS SVC_EXCL_RSN_CD
FROM  &SVC.EVAL&SVC_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C1
ON    C1.CLP_ISA_FILE_ID      = SVC_ISA_FILE_ID
  AND C1.CLP_GS_GRP_SEQ_NO    = SVC_GS_GRP_SEQ_NO
  AND C1.CLP_ST_SEQ_NO        = SVC_ST_SEQ_NO
  AND C1.CLP_LX_SEQ_NO        = SVC_LX_SEQ_NO
  AND C1.CLP_CLM_SEQ_NO       = SVC_CLP_CLM_SEQ_NO
  AND C1.CLP_INCL_RSN_CD     <> 'F03'
  AND C1.CLP_EXCL_RSN_CD      = '   '
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    C2.CLP_ISA_FILE_ID      = C1.CLP_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO    = C1.CLP_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO        = C1.CLP_ST_SEQ_NO
  AND C2.CLP_LX_SEQ_NO        = C1.CLP_LX_SEQ_NO
  AND C2.CLP_MULTI_USE_AMOUNT = C1.CLP_CLM_SEQ_NO
  AND C2.CLP_INCL_RSN_CD      = 'F03'
  AND C2.CLP_EXCL_RSN_CD      = '   '
  AND RTRIM(C2.CLP_RVSD_INVC_NO)  = RTRIM(C1.CLP_CLM_SUBMTR_ID)
		AND RTRIM(C2.CLP_CLM_SUBMTR_ID) = SUBSTR(SVC_RVSD_INVC_NO,1,INSTR(SVC_RVSD_INVC_NO,'|')-1)
INNER JOIN
      CLARITY_TDL_TRAN  A
ON    TX_ID                   = SVC_CLARITY_TX_ID
  AND DETAIL_TYPE             = 50
  AND INVOICE_NUMBER          = RTRIM(C2.CLP_CLM_SUBMTR_ID)
INNER JOIN
      ARPB_TRANSACTIONS B
ON    B.TX_ID                 = A.TX_ID
WHERE SVC_EXCL_RSN_CD         = 'F03'
ORDER BY 1,2,3,4,5,6,7
;
TABLEF FILE SQLOUT
LIST *
END
