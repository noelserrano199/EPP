-STEP001
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP001';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
MERGE INTO &SVC.EVAL&SVC_TBL.EVAL UT
USING (
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
     ,SVC_SEQ_NO
     ,SVC_EPP_GEN_SEQ
     ,T2.ERA_INV
     ,T2.OLD_TX_ID
     ,T2.INVOICE_NUM||'|'||TO_CHAR(T2.CLM_LN)||'|'||TO_CHAR(T2.FROM_SVC_DATE,'YYYYMMDD')||'|'||T2.CL_CPT_CODE
                                       AS RVSD_DATA
     ,T2.TRANSACTION_LIST
     ,T2.CLM_LN
     ,CASE WHEN T2.ERA_STS = '22'
                THEN 0-T2.CHARGE_AMOUNT
           ELSE T2.CHARGE_AMOUNT
      END                              AS CHARGE_AMOUNT
     ,T2.FROM_SVC_DATE
     ,T2.CL_CPT_CODE
FROM
(
SELECT DISTINCT
      RTRIM(CLP_CLM_SUBMTR_ID)         AS ERA_INV
     ,RTRIM(SVC_PROC1_CD)              AS ERA_CPT
     ,TO_DATE(SVC_DTM_DATE,'YYYYMMDD') AS ERA_DOS
     ,ABS(TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0')))
                                       AS ERA_CRG
     ,RTRIM(CLP_CLM_STATUS_CD)         AS ERA_STS
     ,DENSE_RANK()
      OVER ( PARTITION BY RTRIM(CLP_CLM_SUBMTR_ID)
                         ,RTRIM(CLP_CLM_STATUS_CD)
                         ,TO_DATE(SVC_DTM_DATE, 'YYYYMMDD')
                         ,RTRIM(SVC_PROC1_CD)
                         ,ABS(TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0')))
             ORDER BY     RTRIM(CLP_CLM_SUBMTR_ID)
                         ,RTRIM(CLP_CLM_STATUS_CD)
                         ,TO_DATE(SVC_DTM_DATE, 'YYYYMMDD')
                         ,RTRIM(SVC_PROC1_CD)
                         ,ABS(TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0')))
                         ,RTRIM(CLP_CLM_STATUS_CD)
                         ,SVC_ISA_FILE_ID
                         ,SVC_GS_GRP_SEQ_NO
                         ,SVC_ST_SEQ_NO
                         ,SVC_LX_SEQ_NO
                         ,SVC_CLP_CLM_SEQ_NO
                         ,SVC_SEQ_NO )       AS S_RANK
     ,SVC_ISA_FILE_ID
     ,SVC_GS_GRP_SEQ_NO
     ,SVC_ST_SEQ_NO
     ,SVC_LX_SEQ_NO
     ,SVC_CLP_CLM_SEQ_NO
     ,SVC_SEQ_NO
     ,SVC_EPP_GEN_SEQ
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL
ON    CLP_ISA_FILE_ID         = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO       = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO           = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD         = '   '
  AND NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL
      WHERE CLP_CAS_ISA_FILE_ID    = CLP_ISA_FILE_ID
        AND CLP_CAS_GS_GRP_SEQ_NO  = CLP_GS_GRP_SEQ_NO
        AND CLP_CAS_ST_SEQ_NO      = CLP_ST_SEQ_NO
        AND CLP_CAS_LX_SEQ_NO      = CLP_LX_SEQ_NO
        AND CLP_CAS_CLP_CLM_SEQ_NO = CLP_CLM_SEQ_NO
        AND CLP_CAS_EXCL_RSN_CD    = '   ' )
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL
ON    SVC_ISA_FILE_ID         = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO       = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO           = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO           = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO      = CLP_CLM_SEQ_NO
  AND SVC_EXCL_RSN_CD         = '   '
INNER JOIN
      &SVC_DTM.EVAL&SVC_DTM_TBL.EVAL
ON    SVC_DTM_ISA_FILE_ID     = SVC_ISA_FILE_ID
  AND SVC_DTM_GS_GRP_SEQ_NO   = SVC_GS_GRP_SEQ_NO
  AND SVC_DTM_ST_SEQ_NO       = SVC_ST_SEQ_NO
  AND SVC_DTM_LX_SEQ_NO       = SVC_LX_SEQ_NO
  AND SVC_DTM_CLP_CLM_SEQ_NO  = SVC_CLP_CLM_SEQ_NO
  AND SVC_DTM_SVC_SEQ_NO      = SVC_SEQ_NO
  AND SVC_DTM_DTTM_QLFR       IN( '472','150' )
  AND SVC_DTM_EXCL_RSN_CD     = '   '
) T0
INNER JOIN
(
SELECT /*+ INDEX(L IDX_INV_NUM_CLM_ADDL) NOPARALLEL ORDERED NO_UNNEST */
      ERA_INV
     ,ERA_CPT
     ,ERA_DOS
     ,ERA_CRG
     ,ERA_STS
     ,DENSE_RANK()
      OVER ( PARTITION BY ERA_INV
                         ,ERA_STS
                         ,ERA_DOS
                         ,ERA_CPT
                         ,ERA_CRG
             ORDER BY     ERA_INV
                         ,ERA_STS
                         ,ERA_DOS
                         ,ERA_CPT
                         ,ERA_CRG
                         ,L.INVOICE_NUM
                         ,L.CLM_LN
                         ,L.TRANSACTION_LIST )
                                AS T_RANK
     ,L.INVOICE_NUM
     ,L.CLM_LN
     ,L.TRANSACTION_LIST
     ,L.CHARGE_AMOUNT
     ,L.FROM_SVC_DATE
     ,CASE WHEN LENGTH(L.PROC_OR_REV_CODE) = 3
                THEN L.UB_CPT_CODE
           ELSE L.PROC_OR_REV_CODE
      END                       AS CL_CPT_CODE
     ,T1.OLD_TX_ID
FROM
(
SELECT /*+ INDEX(A IDX_CLARITY_TDL_TRAN_INV_C) INDEX(C CLARITY_TDL_TRAN_PAT_ID) INDEX(E CLARITY_TDL_TRAN_PAT_ID) INDEX(G CLARITY_TDL_TRAN_PAT_ID) INDEX(I CLARITY_TDL_TRAN_PAT_ID) NOPARALLEL ORDERED NO_UNNEST */ DISTINCT
      RTRIM(CLP_CLM_SUBMTR_ID)         AS ERA_INV
     ,RTRIM(SVC_PROC1_CD)              AS ERA_CPT
     ,TO_DATE(SVC_DTM_DATE,'YYYYMMDD') AS ERA_DOS
     ,ABS(TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0')))
                                       AS ERA_CRG
     ,RTRIM(CLP_CLM_STATUS_CD)         AS ERA_STS
     ,A.TX_ID                          AS OLD_TX_ID
     ,CAST(COALESCE(J.TX_ID,H.TX_ID,F.TX_ID,D.TX_ID) AS INTEGER)
                                       AS NEW_TX_ID
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL
ON    CLP_ISA_FILE_ID         = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO       = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO           = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD         = '   '
  AND NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM  &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL
      WHERE CLP_CAS_ISA_FILE_ID    = CLP_ISA_FILE_ID
        AND CLP_CAS_GS_GRP_SEQ_NO  = CLP_GS_GRP_SEQ_NO
        AND CLP_CAS_ST_SEQ_NO      = CLP_ST_SEQ_NO
        AND CLP_CAS_LX_SEQ_NO      = CLP_LX_SEQ_NO
        AND CLP_CAS_CLP_CLM_SEQ_NO = CLP_CLM_SEQ_NO
        AND CLP_CAS_EXCL_RSN_CD    = '   ' )
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL
ON    SVC_ISA_FILE_ID         = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO       = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO           = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO           = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO      = CLP_CLM_SEQ_NO
  AND SVC_EXCL_RSN_CD         = '   '
INNER JOIN
      &SVC_DTM.EVAL&SVC_DTM_TBL.EVAL
ON    SVC_DTM_ISA_FILE_ID     = SVC_ISA_FILE_ID
  AND SVC_DTM_GS_GRP_SEQ_NO   = SVC_GS_GRP_SEQ_NO
  AND SVC_DTM_ST_SEQ_NO       = SVC_ST_SEQ_NO
  AND SVC_DTM_LX_SEQ_NO       = SVC_LX_SEQ_NO
  AND SVC_DTM_CLP_CLM_SEQ_NO  = SVC_CLP_CLM_SEQ_NO
  AND SVC_DTM_SVC_SEQ_NO      = SVC_SEQ_NO
  AND SVC_DTM_DTTM_QLFR       IN( '472','150' )
  AND SVC_DTM_EXCL_RSN_CD     = '   '
INNER JOIN
      CLARITY_TDL_TRAN A
ON    ( A.INVOICE_NUMBER        = RTRIM(CLP_CLM_SUBMTR_ID)
    AND A.CPT_CODE              = RTRIM(SVC_PROC1_CD)
    AND A.ORIG_SERVICE_DATE     = TO_DATE(SVC_DTM_DATE,'YYYYMMDD')
    AND A.ORIG_AMT              = ABS(TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0')))
    AND A.DETAIL_TYPE           = 50 )
  OR (  A.INVOICE_NUMBER        = RTRIM(CLP_CLM_SUBMTR_ID)
    AND A.CPT_CODE              = RTRIM(SVC_PROC1_CD)
    AND A.ORIG_SERVICE_DATE     = TO_DATE(SVC_DTM_DATE,'YYYYMMDD')
    AND A.DETAIL_TYPE           = 50
    AND NOT EXISTS
		    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
        FROM  CLARITY_TDL_TRAN X
        WHERE X.INVOICE_NUMBER        = RTRIM(CLP_CLM_SUBMTR_ID)
          AND X.CPT_CODE              = RTRIM(SVC_PROC1_CD)
          AND X.ORIG_SERVICE_DATE     = TO_DATE(SVC_DTM_DATE,'YYYYMMDD')
          AND X.ORIG_AMT              = ABS(TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0')))
          AND X.DETAIL_TYPE           = 50 ))
INNER JOIN
      ARPB_TRANSACTIONS B
ON    B.TX_ID                 = A.TX_ID
  AND B.VOID_DATE             IS NOT NULL
  AND B.REPOST_TYPE_C         IS NOT NULL
INNER JOIN
      CLARITY_TDL_TRAN  C
ON    C.INT_PAT_ID            = A.INT_PAT_ID
  AND C.DETAIL_TYPE           = 1
  AND ( C.CPT_CODE            = A.CPT_CODE
    OR  C.ORIG_SERVICE_DATE   = A.ORIG_SERVICE_DATE
    OR  C.ORIG_AMT            = A.ORIG_AMT )
INNER JOIN
      ARPB_TRANSACTIONS D
ON    D.TX_ID                 = C.TX_ID
  AND D.REPOST_ETR_ID         = A.TX_ID
LEFT OUTER JOIN
      CLARITY_TDL_TRAN  E
ON    D.REPOST_TYPE_C         IS NOT NULL
  AND E.INT_PAT_ID            = A.INT_PAT_ID
  AND E.DETAIL_TYPE           = 1
  AND ( E.CPT_CODE            = A.CPT_CODE
    OR  E.ORIG_SERVICE_DATE   = A.ORIG_SERVICE_DATE
    OR  E.ORIG_AMT            = A.ORIG_AMT )
LEFT OUTER JOIN
      ARPB_TRANSACTIONS F
ON    F.TX_ID                 = E.TX_ID
  AND F.REPOST_ETR_ID         = D.TX_ID
LEFT OUTER JOIN
      CLARITY_TDL_TRAN  G
ON    F.REPOST_TYPE_C         IS NOT NULL
  AND G.INT_PAT_ID            = A.INT_PAT_ID
  AND G.DETAIL_TYPE           = 1
  AND ( G.CPT_CODE            = A.CPT_CODE
    OR  G.ORIG_SERVICE_DATE   = A.ORIG_SERVICE_DATE
    OR  G.ORIG_AMT            = A.ORIG_AMT )
LEFT OUTER JOIN
      ARPB_TRANSACTIONS H
ON    H.TX_ID                 = G.TX_ID
  AND H.REPOST_ETR_ID         = F.TX_ID
LEFT OUTER JOIN
      CLARITY_TDL_TRAN  I
ON    H.REPOST_TYPE_C         IS NOT NULL
  AND I.INT_PAT_ID            = A.INT_PAT_ID
  AND I.DETAIL_TYPE           = 1
  AND ( I.CPT_CODE            = A.CPT_CODE
    OR  I.ORIG_SERVICE_DATE   = A.ORIG_SERVICE_DATE
    OR  I.ORIG_AMT            = A.ORIG_AMT )
LEFT OUTER JOIN
      ARPB_TRANSACTIONS J
ON    J.TX_ID                 = I.TX_ID
  AND J.REPOST_ETR_ID         = H.TX_ID
WHERE ST_EXCL_RSN_CD          = '   '
ORDER BY 1,2
) T1
INNER JOIN
      CLARITY_TDL_TRAN K
ON    K.TX_ID                 = T1.NEW_TX_ID
  AND K.DETAIL_TYPE           = 50
  AND K.INVOICE_NUMBER       <> T1.ERA_INV
INNER JOIN
      ARPB_TRANSACTIONS Q
ON    Q.TX_ID                 = K.TX_ID
  AND Q.VOID_DATE             IS NULL
INNER JOIN
      INV_CLM_LN_ADDL  L
ON    L.INVOICE_NUM           = K.INVOICE_NUMBER
  AND 0                       = INSTR(L.TRANSACTION_LIST,',')
  AND TO_CHAR(K.TX_ID)        IN(L.TRANSACTION_LIST)
  AND L.CLAIM_STATUS_C        IN(1,2)
INNER JOIN
      INV_BASIC_INFO   M
ON    M.INV_ID                = L.INVOICE_ID
 AND  M.LINE                  = L.INV_NUM_GRP100LN
 AND  M.INV_STATUS_C          IN(2,6)
 AND  M.LINE  = ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
                        MIN(X.LINE)
                  FROM  INV_BASIC_INFO X
                  WHERE X.INV_ID       = L.INVOICE_ID
                    AND X.INV_STATUS_C IN(2,6))
ORDER BY 1,2,3,4,5,6
) T2
ON    T2.ERA_INV              = T0.ERA_INV
  AND T2.ERA_STS              = T0.ERA_STS
  AND T2.ERA_DOS              = T0.ERA_DOS
  AND T2.ERA_CPT              = T0.ERA_CPT
  AND T2.ERA_CRG              = T0.ERA_CRG
  AND T2.T_RANK               = T0.S_RANK
ORDER BY 1,2,3,4,5,6,7
) ST
ON  ( ST.SVC_ISA_FILE_ID      = UT.SVC_ISA_FILE_ID
  AND ST.SVC_GS_GRP_SEQ_NO    = UT.SVC_GS_GRP_SEQ_NO
  AND ST.SVC_ST_SEQ_NO        = UT.SVC_ST_SEQ_NO
  AND ST.SVC_LX_SEQ_NO        = UT.SVC_LX_SEQ_NO
  AND ST.SVC_CLP_CLM_SEQ_NO   = UT.SVC_CLP_CLM_SEQ_NO
  AND ST.SVC_SEQ_NO           = UT.SVC_SEQ_NO
  AND ST.SVC_EPP_GEN_SEQ      = UT.SVC_EPP_GEN_SEQ )
WHEN MATCHED THEN
UPDATE SET
      UT.SVC_CLARITY_TX_ID    = ST.TRANSACTION_LIST
     ,UT.SVC_RVSD_INVC_NO     = ST.RVSD_DATA
     ,UT.SVC_MULTI_USE_AMOUNT = ST.CHARGE_AMOUNT
     ,UT.SVC_EXCL_RSN_CD      = 'F03'
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
