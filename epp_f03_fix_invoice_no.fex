-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
MERGE INTO &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL UT
USING (
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      RTRIM(C1.CLP_CLM_SUBMTR_ID)
     ,SC2.SVC_CAS_ISA_FILE_ID
     ,SC2.SVC_CAS_GS_GRP_SEQ_NO
     ,SC2.SVC_CAS_ST_SEQ_NO
     ,SC2.SVC_CAS_LX_SEQ_NO
     ,SC2.SVC_CAS_CLP_CLM_SEQ_NO
     ,SC2.SVC_CAS_SVC_SEQ_NO
     ,SC2.SVC_CAS_SEQ_NO
     ,SC2.SVC_CAS_EPP_GEN_SEQ
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL  C1
ON    C1.CLP_ISA_FILE_ID        = ST_ISA_FILE_ID
  AND C1.CLP_GS_GRP_SEQ_NO      = ST_GS_GRP_SEQ_NO
  AND C1.CLP_ST_SEQ_NO          = ST_SEQ_NO
  AND C1.CLP_INCL_RSN_CD        = 'F03'
  AND C1.CLP_EXCL_RSN_CD        = '   '
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL  C2
ON    C2.CLP_ISA_FILE_ID        = C1.CLP_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO      = C1.CLP_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO          = C1.CLP_ST_SEQ_NO
  AND C2.CLP_LX_SEQ_NO          = C1.CLP_LX_SEQ_NO
  AND C2.CLP_CLM_SEQ_NO         = C1.CLP_MULTI_USE_AMOUNT
  AND C2.CLP_INCL_RSN_CD       <> 'F03'
  AND C2.CLP_EXCL_RSN_CD        = 'F03'
-*
-*  AND RTRIM(CLP_CLM_SUBMTR_ID)  = 'P1309641480'
-*
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL S1
ON    S1.SVC_ISA_FILE_ID        = C1.CLP_ISA_FILE_ID
  AND S1.SVC_GS_GRP_SEQ_NO      = C1.CLP_GS_GRP_SEQ_NO
  AND S1.SVC_ST_SEQ_NO          = C1.CLP_ST_SEQ_NO
  AND S1.SVC_LX_SEQ_NO          = C1.CLP_LX_SEQ_NO
  AND S1.SVC_CLP_CLM_SEQ_NO     = C1.CLP_CLM_SEQ_NO
  AND S1.SVC_EXCL_RSN_CD        = '   '
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL S2
ON    S2.SVC_ISA_FILE_ID        = C2.CLP_ISA_FILE_ID
  AND S2.SVC_GS_GRP_SEQ_NO      = C2.CLP_GS_GRP_SEQ_NO
  AND S2.SVC_ST_SEQ_NO          = C2.CLP_ST_SEQ_NO
  AND S2.SVC_LX_SEQ_NO          = C2.CLP_LX_SEQ_NO
  AND S2.SVC_CLP_CLM_SEQ_NO     = C2.CLP_CLM_SEQ_NO
  AND S2.SVC_SEQ_NO             = S1.SVC_SEQ_NO
  AND S2.SVC_EXCL_RSN_CD        = 'F03'
  AND ABS(TO_NUMBER(COALESCE(RTRIM(S2.SVC_ORGLI_PRVDR_PYMT),'0')))
                               <>  ABS(TO_NUMBER(COALESCE(RTRIM(S1.SVC_ORGLI_PRVDR_PYMT),'0')))
INNER JOIN
      &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL SC1
ON    SVC_CAS_ISA_FILE_ID       = S1.SVC_ISA_FILE_ID
  AND SVC_CAS_GS_GRP_SEQ_NO     = S1.SVC_GS_GRP_SEQ_NO
  AND SVC_CAS_ST_SEQ_NO         = S1.SVC_ST_SEQ_NO
  AND SVC_CAS_LX_SEQ_NO         = S1.SVC_LX_SEQ_NO
  AND SVC_CAS_CLP_CLM_SEQ_NO    = S1.SVC_CLP_CLM_SEQ_NO
  AND SVC_CAS_SVC_SEQ_NO        = S1.SVC_SEQ_NO
  AND SVC_CAS_EXCL_RSN_CD       = '   '
  AND ABS(TO_NUMBER(COALESCE(RTRIM(SVC_CAS_CLM_ADJ_AMT1),'0')))
                                = ABS(TO_NUMBER(COALESCE(RTRIM(S2.SVC_ORGLI_PRVDR_PYMT),'0')))
INNER JOIN
      &SVC_CAS.EVAL&SVC_CAS_TBL.EVAL SC2
ON    SC2.SVC_CAS_ISA_FILE_ID   = S1.SVC_ISA_FILE_ID
  AND SC2.SVC_CAS_GS_GRP_SEQ_NO = S1.SVC_GS_GRP_SEQ_NO
  AND SC2.SVC_CAS_ST_SEQ_NO     = S1.SVC_ST_SEQ_NO
  AND SC2.SVC_CAS_LX_SEQ_NO     = S1.SVC_LX_SEQ_NO
  AND SC2.SVC_CAS_CLP_CLM_SEQ_NO= S1.SVC_CLP_CLM_SEQ_NO
  AND SC2.SVC_CAS_SVC_SEQ_NO    = S1.SVC_SEQ_NO
  AND SC2.SVC_CAS_EXCL_RSN_CD   = '   '
WHERE ST_EXCL_RSN_CD            = '   '
) ST
ON  ( ST.SVC_CAS_ISA_FILE_ID    = UT.SVC_CAS_ISA_FILE_ID
  AND ST.SVC_CAS_GS_GRP_SEQ_NO  = UT.SVC_CAS_GS_GRP_SEQ_NO
  AND ST.SVC_CAS_ST_SEQ_NO      = UT.SVC_CAS_ST_SEQ_NO
  AND ST.SVC_CAS_LX_SEQ_NO      = UT.SVC_CAS_LX_SEQ_NO
  AND ST.SVC_CAS_CLP_CLM_SEQ_NO = UT.SVC_CAS_CLP_CLM_SEQ_NO
  AND ST.SVC_CAS_SVC_SEQ_NO     = UT.SVC_CAS_SVC_SEQ_NO
  AND ST.SVC_CAS_SEQ_NO         = UT.SVC_CAS_SEQ_NO
  AND ST.SVC_CAS_EPP_GEN_SEQ    = UT.SVC_CAS_EPP_GEN_SEQ )
WHEN MATCHED THEN
UPDATE SET
      UT.SVC_CAS_EXCL_RSN_CD    = 'F03'
     ,UT.SVC_CAS_PATIENT_PYMT   = '-2222'
;
TABLEF FILE SQLOUT
LIST *
END
