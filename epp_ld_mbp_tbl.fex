-STEP004
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP004';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-TYPE &NOWTIME.EVAL *** Insert OTLT rows to &OTLT.EVAL&OTLT_TBL.EVAL
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &MBP.EVAL&MBP_TBL.EVAL
SELECT RTRIM(FEXNAME)
      ,RTRIM(PAYOR)
FROM   TEMP_MBP_INPUT
ORDER BY 1
;
END
