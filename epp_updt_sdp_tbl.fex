-*-----------------------------------------------------------------------------
SQL
DELETE
FROM &SDP.EVAL&SDP_TBL.EVAL
WHERE EXISTS
      ( SELECT 1
        FROM   &ISA.EVAL&ISA_TBL.EVAL
        WHERE  ISA_FILE_ID   = SDP_IFCT_FILE_ID);
COMMIT WORK;
END
