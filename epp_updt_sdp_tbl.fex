-STEP002
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP002';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SDP.EVAL&SDP_TBL.EVAL
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      TRUNC(CURRENT_DATE)   AS SDP_PROCESS_DATE
     ,SVC_CLARITY_TX_ID     AS SDP_CLARITY_TX_ID
     ,IFCT_FILE_ID          AS SDP_IFCT_FILE_ID
     ,IFCT_FILE_PAYER       AS SDP_IFCT_FILE_PAYER
     ,SUM(TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0')))
                            AS SDP_PAYMENT_AMT 
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &IFCT.EVAL&IFCT_TBL.EVAL
ON    IFCT_FILE_ID           = ST_ISA_FILE_ID
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C1
ON    CLP_ISA_FILE_ID        = ST_ISA_FILE_ID
  AND CLP_GS_GRP_SEQ_NO      = ST_GS_GRP_SEQ_NO
  AND CLP_ST_SEQ_NO          = ST_SEQ_NO
  AND CLP_EXCL_RSN_CD        = '   '
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL SVC1
ON    SVC_ISA_FILE_ID        = CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO      = CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO          = CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO          = CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO     = CLP_CLM_SEQ_NO
  AND SVC_EXCL_RSN_CD        = '   '
  AND RTRIM(SVC_CLARITY_TX_ID)    <> '0'
  AND TO_NUMBER(SVC_CLARITY_TX_ID) > 1000
  AND TO_NUMBER(COALESCE(RTRIM(SVC_LI_PRVDR_PYMT),'0')) > 0
WHERE ST_EXCL_RSN_CD         = '   '
GROUP BY
      TRUNC(CURRENT_DATE)
     ,SVC_CLARITY_TX_ID
     ,IFCT_FILE_ID
     ,IFCT_FILE_PAYER
ORDER BY 1,2,3
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
