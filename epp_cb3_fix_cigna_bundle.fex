-*     ,CLP_CLM_SUBMTR_ID
     ,CAST( TO_NUMBER(COALESCE(RTRIM(CLP_CLM_PYMT_AMT),'0'))
       *( TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0'))
            /TO_NUMBER(COALESCE(RTRIM(CLP_TOT_CLM_CRG_AMT),'0'))) AS NUMBER(10,2))
                                 AS SVC_LI_PRVDR_PYMT
FROM  &CLP.EVAL&CLP_TBL.EVAL
INNER JOIN
