-STEP019
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP019';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL Insert SVC segements
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC.EVAL&SVC_TBL.EVAL
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
-*      C1.CLP_CLM_SUBMTR_ID
      C2.CLP_ISA_FILE_ID             AS SVC_ISA_FILE_ID
     ,C2.CLP_GS_GRP_SEQ_NO           AS SVC_GS_GRP_SEQ_NO
     ,C2.CLP_ST_SEQ_NO               AS SVC_ST_SEQ_NO
     ,C2.CLP_LX_SEQ_NO               AS SVC_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO              AS SVC_CLP_CLM_SEQ_NO
     ,SVC_SEQ_NO
     ,SVC_EPP_GEN_SEQ
     ,SVC_COMP_SEPERATOR
     ,SVC_PROC1_SVC_ID_QLFR
     ,SVC_PROC1_CD
     ,SVC_PROC1_MODF1
     ,SVC_PROC1_MODF2
     ,SVC_PROC1_MODF3
     ,SVC_PROC1_MODF4
     ,0-TO_NUMBER(COALESCE(RTRIM(SVC_ORGLI_PRVDR_PYMT),'0'))
                                     AS SVC_ORGLI_PRVDR_PYMT
     ,TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0'))
                                     AS SVC_LI_PRVDR_PYMT
     ,SVC_NTL_BILNG_REV_CD
     ,SVC_UNTS_SVC_PD_CNT
     ,SVC_PROC2_SVC_ID_QLFR
     ,SVC_PROC2_CD
     ,SVC_PROC2_MODF1
     ,SVC_PROC2_MODF2
     ,SVC_PROC2_MODF3
     ,SVC_PROC2_MODF4
     ,SVC_PROC2_CD_DESC
     ,SVC_ORG_UNTS_SVC_CNT
     ,SVC_CLARITY_TX_ID
     ,SVC_TX_ID_MATCH_TYPE
     ,SVC_CONTR_PAYOR_IND
     ,SVC_DUP_PMT_IND
     ,SVC_RVSD_INVC_NO
     ,SVC_CURRENT_BALANCE
     ,SVC_INS_AMOUNT_PAID
     ,SVC_INS_AMOUNT_ADJ
     ,SVC_CRG_IS_VOID_YN
     ,SVC_PRIOR_DENIAL_YN
     ,TO_NUMBER(COALESCE(RTRIM(CLP_CAS_CLM_ADJ_AMT1),'0'))
                                     AS SVC_MULTI_USE_AMOUNT
     ,SVC_SCENARIO
     ,'CA3'                          AS SVC_INCL_RSN_CD
     ,'   '                          AS SVC_EXCL_RSN_CD
FROM  &CLP.EVAL&CLP_TBL.EVAL C1
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    C2.CLP_ISA_FILE_ID          = C1.CLP_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO        = C1.CLP_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO            = C1.CLP_ST_SEQ_NO
  AND C2.CLP_EXCL_RSN_CD          = 'CA3'
  AND RTRIM(C2.CLP_CLM_STATUS_CD) = '22'
  AND TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0')) <> 0
  AND RTRIM(C2.CLP_CLM_SUBMTR_ID) = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND TO_NUMBER(COALESCE(RTRIM(C2.CLP_TOT_CLM_CRG_AMT),'0'))
                                  = 0-C1.CLP_MULTI_USE_AMOUNT
INNER JOIN
      &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL CC2
ON    CC2.CLP_CAS_ISA_FILE_ID       = C2.CLP_ISA_FILE_ID
  AND CC2.CLP_CAS_GS_GRP_SEQ_NO     = C2.CLP_GS_GRP_SEQ_NO
  AND CC2.CLP_CAS_ST_SEQ_NO         = C2.CLP_ST_SEQ_NO
  AND CC2.CLP_CAS_LX_SEQ_NO         = C2.CLP_LX_SEQ_NO
  AND CC2.CLP_CAS_CLP_CLM_SEQ_NO    = C2.CLP_CLM_SEQ_NO
  AND CC2.CLP_CAS_CLM_ADJ_GRP_CD1   = 'CO'
  AND CC2.CLP_CAS_EXCL_RSN_CD       = 'CA3'
  AND RTRIM(CC2.CLP_CAS_CLM_ADJ_RSN_CD1) = '45'
INNER JOIN
      &SVC.EVAL&SVC_TBL.EVAL
ON    SVC_ISA_FILE_ID             = C1.CLP_ISA_FILE_ID
  AND SVC_GS_GRP_SEQ_NO           = C1.CLP_GS_GRP_SEQ_NO
  AND SVC_ST_SEQ_NO               = C1.CLP_ST_SEQ_NO
  AND SVC_LX_SEQ_NO               = C1.CLP_LX_SEQ_NO
  AND SVC_CLP_CLM_SEQ_NO          = C1.CLP_CLM_SEQ_NO
  AND SVC_INCL_RSN_CD             = 'CA3'
  AND SVC_EXCL_RSN_CD             = '   '
WHERE C1.CLP_INCL_RSN_CD          = 'CA3'
  AND RTRIM(C1.CLP_CLM_STATUS_CD) = '1'
ORDER BY 1,2,3,4,5,6,7
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
