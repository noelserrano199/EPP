-STEP001
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP001';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL -
-TYPE &NOWTIME.EVAL &STEP.EVAL -
-*-----------------------------------------------------------------------------
SQL
MERGE INTO &CLP.EVAL&CLP_TBL.EVAL UT
USING (
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
      C1.CLP_CLM_SUBMTR_ID
     ,C2.CLP_ISA_FILE_ID
     ,C2.CLP_GS_GRP_SEQ_NO
     ,C2.CLP_ST_SEQ_NO
     ,C2.CLP_LX_SEQ_NO
     ,C2.CLP_CLM_SEQ_NO
     ,C2.CLP_EPP_GEN_SEQ
FROM  &ST.EVAL&ST_TBL.EVAL
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C1
ON    C1.CLP_ISA_FILE_ID            = ST_ISA_FILE_ID
  AND C1.CLP_GS_GRP_SEQ_NO          = ST_GS_GRP_SEQ_NO
  AND C1.CLP_ST_SEQ_NO              = ST_SEQ_NO
  AND C1.CLP_EXCL_RSN_CD            = '   '
  AND RTRIM(C1.CLP_CLM_STATUS_CD)   = '2'
  AND TO_NUMBER(COALESCE(RTRIM(C1.CLP_CLM_PYMT_AMT),'0')) <> 0
  AND NOT EXISTS
    ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
      FROM   &SVC.EVAL&SVC_TBL.EVAL
      WHERE  SVC_ISA_FILE_ID        = C1.CLP_ISA_FILE_ID
        AND  SVC_GS_GRP_SEQ_NO      = C1.CLP_GS_GRP_SEQ_NO
        AND  SVC_ST_SEQ_NO          = C1.CLP_ST_SEQ_NO
        AND  SVC_LX_SEQ_NO          = C1.CLP_LX_SEQ_NO
        AND  SVC_CLP_CLM_SEQ_NO     = C1.CLP_CLM_SEQ_NO
        AND  SVC_EXCL_RSN_CD        = '   ' )
INNER JOIN
      &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL CC1
ON    CC1.CLP_CAS_ISA_FILE_ID       = C1.CLP_ISA_FILE_ID
  AND CC1.CLP_CAS_GS_GRP_SEQ_NO     = C1.CLP_GS_GRP_SEQ_NO
  AND CC1.CLP_CAS_ST_SEQ_NO         = C1.CLP_ST_SEQ_NO
  AND CC1.CLP_CAS_LX_SEQ_NO         = C1.CLP_LX_SEQ_NO
  AND CC1.CLP_CAS_CLP_CLM_SEQ_NO    = C1.CLP_CLM_SEQ_NO
  AND CC1.CLP_CAS_CLM_ADJ_GRP_CD1   = 'CO'
  AND CC1.CLP_CAS_EXCL_RSN_CD       = '   '
  AND RTRIM(CC1.CLP_CAS_CLM_ADJ_RSN_CD1) = '45'
  AND NOT EXISTS
( SELECT 1
  FROM   &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL CCX
  WHERE  CCX.CLP_CAS_ISA_FILE_ID    = C1.CLP_ISA_FILE_ID
    AND  CCX.CLP_CAS_GS_GRP_SEQ_NO  = C1.CLP_GS_GRP_SEQ_NO
    AND  CCX.CLP_CAS_ST_SEQ_NO      = C1.CLP_ST_SEQ_NO
    AND  CCX.CLP_CAS_LX_SEQ_NO      = C1.CLP_LX_SEQ_NO
    AND  CCX.CLP_CAS_CLP_CLM_SEQ_NO = C1.CLP_CLM_SEQ_NO
    AND  CCX.CLP_CAS_EXCL_RSN_CD    = '   '
    AND  CCX.CLP_CAS_SEQ_NO        <> CC1.CLP_CAS_SEQ_NO )
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    C2.CLP_ISA_FILE_ID            = ST_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO          = ST_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO              = ST_SEQ_NO
  AND C2.CLP_EXCL_RSN_CD            = '   '
  AND RTRIM(C2.CLP_CLM_STATUS_CD)   = '22'
  AND TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0')) <> 0
  AND RTRIM(C2.CLP_CLM_SUBMTR_ID)   = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND TO_NUMBER(COALESCE(RTRIM(C2.CLP_TOT_CLM_CRG_AMT),'0'))
       = 0-TO_NUMBER(COALESCE(RTRIM(C1.CLP_TOT_CLM_CRG_AMT),'0'))
INNER JOIN
      &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL CC2
ON    CC2.CLP_CAS_ISA_FILE_ID       = C2.CLP_ISA_FILE_ID
  AND CC2.CLP_CAS_GS_GRP_SEQ_NO     = C2.CLP_GS_GRP_SEQ_NO
  AND CC2.CLP_CAS_ST_SEQ_NO         = C2.CLP_ST_SEQ_NO
  AND CC2.CLP_CAS_LX_SEQ_NO         = C2.CLP_LX_SEQ_NO
  AND CC2.CLP_CAS_CLP_CLM_SEQ_NO    = C2.CLP_CLM_SEQ_NO
  AND CC2.CLP_CAS_CLM_ADJ_GRP_CD1   = 'CO'
  AND CC2.CLP_CAS_EXCL_RSN_CD       = '   '
  AND RTRIM(CC2.CLP_CAS_CLM_ADJ_RSN_CD1)= '45'
  AND NOT EXISTS
( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */ 1
  FROM   &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL CCY
  WHERE  CCY.CLP_CAS_ISA_FILE_ID    = C2.CLP_ISA_FILE_ID
    AND  CCY.CLP_CAS_GS_GRP_SEQ_NO  = C2.CLP_GS_GRP_SEQ_NO
    AND  CCY.CLP_CAS_ST_SEQ_NO      = C2.CLP_ST_SEQ_NO
    AND  CCY.CLP_CAS_LX_SEQ_NO      = C2.CLP_LX_SEQ_NO
    AND  CCY.CLP_CAS_CLP_CLM_SEQ_NO = C2.CLP_CLM_SEQ_NO
    AND  CCY.CLP_CAS_EXCL_RSN_CD    = '   '
    AND  CCY.CLP_CAS_SEQ_NO        <> CC2.CLP_CAS_SEQ_NO )
INNER JOIN
      CLARITY_TDL_TRAN  A
ON    A.INVOICE_NUMBER              = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND A.DETAIL_TYPE                 = 50
INNER JOIN
      ARPB_TRANSACTIONS B
ON    B.TX_ID                       = A.TX_ID
  AND B.OUTSTANDING_AMT             = 0
  AND B.VOID_DATE                   IS NULL
  AND (B.TOTAL_MTCH_INS_AMT-B.TOTAL_MTCH_INS_ADJ)
        < TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0'))
WHERE ST_EXCL_RSN_CD                = '   '
  AND A.TX_ID = ( SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
                       MIN(W.TX_ID)
                  FROM CLARITY_TDL_TRAN  W
                      ,ARPB_TRANSACTIONS X
                  WHERE W.INVOICE_NUMBER   = RTRIM(C1.CLP_CLM_SUBMTR_ID)
                    AND W.DETAIL_TYPE      = 50
                    AND X.TX_ID            = W.TX_ID
                    AND X.OUTSTANDING_AMT  = 0
                    AND X.VOID_DATE        IS NULL
                    AND (X.TOTAL_MTCH_INS_AMT-X.TOTAL_MTCH_INS_ADJ)
                          < TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0')))
ORDER BY 1,2,3,4,5,6
) ST
ON  (    ST.CLP_ISA_FILE_ID         = UT.CLP_ISA_FILE_ID
     AND ST.CLP_GS_GRP_SEQ_NO       = UT.CLP_GS_GRP_SEQ_NO
     AND ST.CLP_ST_SEQ_NO           = UT.CLP_ST_SEQ_NO
     AND ST.CLP_LX_SEQ_NO           = UT.CLP_LX_SEQ_NO
     AND ST.CLP_CLM_SEQ_NO          = UT.CLP_CLM_SEQ_NO
     AND ST.CLP_EPP_GEN_SEQ         = UT.CLP_EPP_GEN_SEQ  )
WHEN MATCHED THEN
UPDATE SET
      UT.CLP_CL_TX_MATCH_TYPE       = 'CA3'
     ,UT.CLP_EXCL_RSN_CD            = 'CA3'
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
