     AND ST.CLP_EPP_GEN_SEQ         = UT.CLP_EPP_GEN_SEQ  )
WHEN MATCHED THEN
UPDATE SET
      UT.CLP_CL_TX_MATCH_TYPE       = 'CA3'
     ,UT.CLP_EXCL_RSN_CD            = 'CA3'
