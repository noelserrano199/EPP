-STEP004
-SET  &STEP    = '&FOCFEXNAME.EVAL'||'.'||'STEP004';
-SET  &NOWTIME = HHMMSS(A8);
-TYPE &NOWTIME.EVAL &STEP.EVAL Insert SVC segements
-*-----------------------------------------------------------------------------
SQL
INSERT INTO &SVC.EVAL&SVC_TBL.EVAL
SELECT /*+ NOPARALLEL ORDERED NO_UNNEST */
-*      C1.CLP_CLM_SUBMTR_ID
      C1.CLP_ISA_FILE_ID             AS SVC_ISA_FILE_ID
     ,C1.CLP_GS_GRP_SEQ_NO           AS SVC_GS_GRP_SEQ_NO
     ,C1.CLP_ST_SEQ_NO               AS SVC_ST_SEQ_NO
     ,C1.CLP_LX_SEQ_NO               AS SVC_LX_SEQ_NO
     ,C1.CLP_CLM_SEQ_NO              AS SVC_CLP_CLM_SEQ_NO
     ,1                              AS SVC_SEQ_NO
     ,1                              AS SVC_EPP_GEN_SEQ
     ,':'                            AS SVC_COMP_SEPERATOR
     ,'HC'                           AS SVC_PROC1_SVC_ID_QLFR
     ,A.CPT_CODE                     AS SVC_PROC1_CD
     ,SUBSTR(COALESCE(A.MODIFIER_ONE, '  '),1,2)
                                     AS SVC_PROC1_MODF1
     ,SUBSTR(COALESCE(A.MODIFIER_TWO, '  '),1,2)
                                     AS SVC_PROC1_MODF2
     ,SUBSTR(COALESCE(A.MODIFIER_THREE, '  '),1,2)
                                     AS SVC_PROC1_MODF3
     ,SUBSTR(COALESCE(A.MODIFIER_FOUR, '  '),1,2)
                                     AS SVC_PROC1_MODF4
     ,CAST(A.ORIG_PRICE AS NUMBER(10,2))
                                     AS SVC_ORGLI_PRVDR_PYMT
     ,TO_NUMBER(COALESCE(RTRIM(C1.CLP_CLM_PYMT_AMT),'0'))
                                     AS SVC_LI_PRVDR_PYMT
     ,' '                            AS SVC_NTL_BILNG_REV_CD
     ,' '                            AS SVC_UNTS_SVC_PD_CNT
     ,' '                            AS SVC_PROC2_SVC_ID_QLFR
     ,' '                            AS SVC_PROC2_CD
     ,' '                            AS SVC_PROC2_MODF1
     ,' '                            AS SVC_PROC2_MODF2
     ,' '                            AS SVC_PROC2_MODF3
     ,' '                            AS SVC_PROC2_MODF4
     ,' '                            AS SVC_PROC2_CD_DESC
     ,' '                            AS SVC_ORG_UNTS_SVC_CNT
     ,A.TX_ID                        AS SVC_CLARITY_TX_ID
     ,'CA3'                          AS SVC_TX_ID_MATCH_TYPE
     ,'Y'                            AS SVC_CONTR_PAYOR_IND
     ,'N'                            AS SVC_DUP_PMT_IND
     ,'N'                            AS SVC_RVSD_INVC_NO
     ,B.OUTSTANDING_AMT              AS SVC_CURRENT_BALANCE
     ,B.TOTAL_MTCH_INS_AMT           AS SVC_INS_AMOUNT_PAID
     ,B.TOTAL_MTCH_INS_ADJ           AS SVC_INS_AMOUNT_ADJ
     ,CASE WHEN B.VOID_DATE IS NULL
                THEN 'N'
           ELSE 'Y'           END    AS SVC_CRG_IS_VOID_YN
     ,'N'                            AS SVC_PRIOR_DENIAL_YN
     ,TO_NUMBER(COALESCE(RTRIM(CLP_CAS_CLM_ADJ_AMT1),'0'))
                                     AS SVC_MULTI_USE_AMOUNT
     ,'X'                            AS SVC_SCENARIO
     ,'CA3'                          AS SVC_INCL_RSN_CD
     ,'   '                          AS SVC_EXCL_RSN_CD
FROM  &CLP.EVAL&CLP_TBL.EVAL C1
INNER JOIN
      &CLP.EVAL&CLP_TBL.EVAL C2
ON    C2.CLP_ISA_FILE_ID          = C1.CLP_ISA_FILE_ID
  AND C2.CLP_GS_GRP_SEQ_NO        = C1.CLP_GS_GRP_SEQ_NO
  AND C2.CLP_ST_SEQ_NO            = C1.CLP_ST_SEQ_NO
  AND C2.CLP_EXCL_RSN_CD          = 'CA3'
  AND RTRIM(C2.CLP_CLM_STATUS_CD) = '22'
  AND TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0')) <> 0
  AND RTRIM(C2.CLP_CLM_SUBMTR_ID) = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND TO_NUMBER(COALESCE(RTRIM(C2.CLP_TOT_CLM_CRG_AMT),'0'))
       = 0-TO_NUMBER(COALESCE(RTRIM(C1.CLP_TOT_CLM_CRG_AMT),'0'))
INNER JOIN
      &CLP_CAS.EVAL&CLP_CAS_TBL.EVAL CC1
ON    CC1.CLP_CAS_ISA_FILE_ID       = C1.CLP_ISA_FILE_ID
  AND CC1.CLP_CAS_GS_GRP_SEQ_NO     = C1.CLP_GS_GRP_SEQ_NO
  AND CC1.CLP_CAS_ST_SEQ_NO         = C1.CLP_ST_SEQ_NO
  AND CC1.CLP_CAS_LX_SEQ_NO         = C1.CLP_LX_SEQ_NO
  AND CC1.CLP_CAS_CLP_CLM_SEQ_NO    = C1.CLP_CLM_SEQ_NO
  AND CC1.CLP_CAS_CLM_ADJ_GRP_CD1   = 'CO'
  AND CC1.CLP_CAS_EXCL_RSN_CD       = '   '
  AND RTRIM(CC1.CLP_CAS_CLM_ADJ_RSN_CD1) = '45'
INNER JOIN
      &CLP_DTM.EVAL&CLP_DTM_TBL.EVAL D1
ON    D1.CLP_DTM_ISA_FILE_ID      = C1.CLP_ISA_FILE_ID
  AND D1.CLP_DTM_GS_GRP_SEQ_NO    = C1.CLP_GS_GRP_SEQ_NO
  AND D1.CLP_DTM_ST_SEQ_NO        = C1.CLP_ST_SEQ_NO
  AND D1.CLP_DTM_LX_SEQ_NO        = C1.CLP_LX_SEQ_NO
  AND D1.CLP_DTM_CLP_CLM_SEQ_NO   = C1.CLP_CLM_SEQ_NO
  AND D1.CLP_DTM_DTTM_QLFR        = '232'
  AND D1.CLP_DTM_EXCL_RSN_CD      = '   '
LEFT OUTER JOIN
      &CLP_DTM.EVAL&CLP_DTM_TBL.EVAL D2
ON    D2.CLP_DTM_ISA_FILE_ID      = C1.CLP_ISA_FILE_ID
  AND D2.CLP_DTM_GS_GRP_SEQ_NO    = C1.CLP_GS_GRP_SEQ_NO
  AND D2.CLP_DTM_ST_SEQ_NO        = C1.CLP_ST_SEQ_NO
  AND D2.CLP_DTM_LX_SEQ_NO        = C1.CLP_LX_SEQ_NO
  AND D2.CLP_DTM_CLP_CLM_SEQ_NO   = C1.CLP_CLM_SEQ_NO
  AND D2.CLP_DTM_DTTM_QLFR        = '233'
  AND D2.CLP_DTM_EXCL_RSN_CD      = '   '
INNER JOIN
      CLARITY_TDL_TRAN  A
ON    A.INVOICE_NUMBER            = RTRIM(C1.CLP_CLM_SUBMTR_ID)
  AND A.DETAIL_TYPE               = 50
INNER JOIN
      ARPB_TRANSACTIONS B
ON    B.TX_ID                     = A.TX_ID
  AND B.OUTSTANDING_AMT           = 0
  AND B.VOID_DATE                 IS NULL
  AND (B.TOTAL_MTCH_INS_AMT-B.TOTAL_MTCH_INS_ADJ)
        < TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0'))
WHERE C1.CLP_EXCL_RSN_CD          = 'CA3'
  AND RTRIM(C1.CLP_CLM_STATUS_CD) = '2'
  AND A.TX_ID = ( SELECT MIN(W.TX_ID)
                  FROM CLARITY_TDL_TRAN  W
                      ,ARPB_TRANSACTIONS X
                  WHERE W.INVOICE_NUMBER   = RTRIM(C1.CLP_CLM_SUBMTR_ID)
                    AND W.DETAIL_TYPE      = 50
                    AND X.TX_ID            = W.TX_ID
                    AND X.OUTSTANDING_AMT  = 0
                    AND X.VOID_DATE        IS NULL
                    AND (X.TOTAL_MTCH_INS_AMT-X.TOTAL_MTCH_INS_ADJ)
                          < TO_NUMBER(COALESCE(RTRIM(C2.CLP_CLM_PYMT_AMT),'0')))
ORDER BY 1,2,3,4,5,6,7
;
TABLEF FILE SQLOUT
LIST *
END
-RUN
